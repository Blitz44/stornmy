fakelag:Element("ToggleColor", "visualize lag", {default = {Toggle = false, Color = COL3RGB(255,255,255)}}, function(tbl)  -- blast#6645 / BlastPipe made the scrolling work lol
	if tbl.Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
		for _,obj in pairs(FakelagFolder:GetChildren()) do  -- blast#6645 / BlastPipe made the scrolling work lol
			obj.Color = tbl.Color  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	else  -- blast#6645 / BlastPipe made the scrolling work lol
		FakelagFolder:ClearAllChildren()  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
end)  -- blast#6645 / BlastPipe made the scrolling work lol
fakelag:Element("ToggleKeybind", "ping spike")  -- blast#6645 / BlastPipe made the scrolling work lol
coroutine.wrap(function()  -- blast#6645 / BlastPipe made the scrolling work lol
	while wait(1/16) do  -- blast#6645 / BlastPipe made the scrolling work lol
		LagTick = CLAMP(LagTick + 1, 0, values.rage.fakelag.limit.Slider)  -- blast#6645 / BlastPipe made the scrolling work lol
		if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("UpperTorso") and values.rage.fakelag.enabled.Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
			if LagTick == (values.rage.fakelag.amount.Dropdown == "static" and values.rage.fakelag.limit.Slider or RANDOM(1, values.rage.fakelag.limit.Slider)) then  -- blast#6645 / BlastPipe made the scrolling work lol
				game:GetService("NetworkClient"):SetOutgoingKBPSLimit(9e9)  -- blast#6645 / BlastPipe made the scrolling work lol
				FakelagFolder:ClearAllChildren()  -- blast#6645 / BlastPipe made the scrolling work lol
				LagTick = 0  -- blast#6645 / BlastPipe made the scrolling work lol
				if values.rage.fakelag["visualize lag"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
					for _,hitbox in pairs(LocalPlayer.Character:GetChildren()) do  -- blast#6645 / BlastPipe made the scrolling work lol
						if hitbox:IsA("BasePart") and hitbox.Name ~= "HumanoidRootPart" then  -- blast#6645 / BlastPipe made the scrolling work lol
							local part = INST("Part")  -- blast#6645 / BlastPipe made the scrolling work lol
							part.CFrame = hitbox.CFrame  -- blast#6645 / BlastPipe made the scrolling work lol
							part.Anchored = true  -- blast#6645 / BlastPipe made the scrolling work lol
							part.CanCollide = false  -- blast#6645 / BlastPipe made the scrolling work lol
							part.Material = Enum.Material.ForceField  -- blast#6645 / BlastPipe made the scrolling work lol
							part.Color = values.rage.fakelag["visualize lag"].Color  -- blast#6645 / BlastPipe made the scrolling work lol
							part.Name = hitbox.Name  -- blast#6645 / BlastPipe made the scrolling work lol
							part.Transparency = 0  -- blast#6645 / BlastPipe made the scrolling work lol
							part.Size = hitbox.Size  -- blast#6645 / BlastPipe made the scrolling work lol
							part.Parent = FakelagFolder  -- blast#6645 / BlastPipe made the scrolling work lol
						end  -- blast#6645 / BlastPipe made the scrolling work lol
					end  -- blast#6645 / BlastPipe made the scrolling work lol
				end  -- blast#6645 / BlastPipe made the scrolling work lol
			else  -- blast#6645 / BlastPipe made the scrolling work lol
				if values.rage.fakelag.enabled.Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
					game:GetService("NetworkClient"):SetOutgoingKBPSLimit(1)  -- blast#6645 / BlastPipe made the scrolling work lol
				end  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		else  -- blast#6645 / BlastPipe made the scrolling work lol
			FakelagFolder:ClearAllChildren()  -- blast#6645 / BlastPipe made the scrolling work lol
			game:GetService("NetworkClient"):SetOutgoingKBPSLimit(9e9)  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
end)()  -- blast#6645 / BlastPipe made the scrolling work lol
