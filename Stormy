local function YROTATION(cframe)  -- blast#6645 / BlastPipe made the scrolling work lol
	local x, y, z = cframe:ToOrientation()  -- blast#6645 / BlastPipe made the scrolling work lol
	return CF(cframe.Position) * CFAngles(0,y,0)  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
local function XYROTATION(cframe)  -- blast#6645 / BlastPipe made the scrolling work lol
	local x, y, z = cframe:ToOrientation()  -- blast#6645 / BlastPipe made the scrolling work lol
	return CF(cframe.Position) * CFAngles(x,y,0)  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
