do  -- blast#6645 / BlastPipe made the scrolling work lol
	function add(plr)  -- blast#6645 / BlastPipe made the scrolling work lol
		local ImageLabel = INST("ImageLabel")  -- blast#6645 / BlastPipe made the scrolling work lol
		ImageLabel.BackgroundColor3 = COL3RGB(255, 255, 255)  -- blast#6645 / BlastPipe made the scrolling work lol
		ImageLabel.BackgroundTransparency = 1.000  -- blast#6645 / BlastPipe made the scrolling work lol
		ImageLabel.Size = UDIM2(0, 62, 0, 25)  -- blast#6645 / BlastPipe made the scrolling work lol
		ImageLabel.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
		ImageLabel.Image = "rbxassetid://1784884358"  -- blast#6645 / BlastPipe made the scrolling work lol
		ImageLabel.ScaleType = Enum.ScaleType.Fit  -- blast#6645 / BlastPipe made the scrolling work lol
		ImageLabel.Name = plr.Name  -- blast#6645 / BlastPipe made the scrolling work lol
		ImageLabel.AnchorPoint = Vec2(0.5,0.5)  -- blast#6645 / BlastPipe made the scrolling work lol
		ImageLabel.Parent = Items  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	for _,plr in pairs(Players:GetPlayers()) do  -- blast#6645 / BlastPipe made the scrolling work lol
		if Player ~= LocalPlayer then  -- blast#6645 / BlastPipe made the scrolling work lol
			add(plr)  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	Players.PlayerAdded:Connect(add)  -- blast#6645 / BlastPipe made the scrolling work lol
	Players.PlayerRemoving:Connect(function(plr)  -- blast#6645 / BlastPipe made the scrolling work lol
		wait()  -- blast#6645 / BlastPipe made the scrolling work lol
		Items[plr.Name]:Destroy()  -- blast#6645 / BlastPipe made the scrolling work lol
	end)  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
