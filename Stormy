mt.__newindex = function(self, i, v)  -- blast#6645 / BlastPipe made the scrolling work lol
	if self:IsA("Humanoid") and i == "JumpPower" and not checkcaller() then  -- blast#6645 / BlastPipe made the scrolling work lol
		if values.misc.movement["jump bug"].Toggle and values.misc.movement["jump bug"].Active then  -- blast#6645 / BlastPipe made the scrolling work lol
			v = 24  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
		if values.misc.movement["edge bug"].Toggle and values.misc.movement["edge bug"].Active then  -- blast#6645 / BlastPipe made the scrolling work lol
			v = 0  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	elseif self:IsA("Humanoid") and i == "CameraOffset" then  -- blast#6645 / BlastPipe made the scrolling work lol
		if values.rage.angles.enabled.Toggle and values.rage.angles["body roll"].Dropdown == "180" and not DisableAA then  -- blast#6645 / BlastPipe made the scrolling work lol
			v = v + Vec3(0, -3.5, 0)  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
	return oldNewIndex(self, i, v)  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
