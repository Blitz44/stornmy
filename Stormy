		else  -- blast#6645 / BlastPipe made the scrolling work lol
			NewScope.Enabled = false  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
		local RageGuy  -- blast#6645 / BlastPipe made the scrolling work lol
		if workspace:FindFirstChild("Map") and Client.gun ~= "none" and Client.gun.Name ~= "C4" then  -- blast#6645 / BlastPipe made the scrolling work lol
			if values.rage.aimbot.enabled.Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
