local ads = Client.updateads  -- blast#6645 / BlastPipe made the scrolling work lol
Client.updateads = function(self, ...)  -- blast#6645 / BlastPipe made the scrolling work lol
	local args = {...}  -- blast#6645 / BlastPipe made the scrolling work lol
	coroutine.wrap(function()  -- blast#6645 / BlastPipe made the scrolling work lol
		wait()  -- blast#6645 / BlastPipe made the scrolling work lol
		if LocalPlayer.Character ~= nil then  -- blast#6645 / BlastPipe made the scrolling work lol
			for _,part in pairs(LocalPlayer.Character:GetDescendants()) do  -- blast#6645 / BlastPipe made the scrolling work lol
				if part:IsA("Part") or part:IsA("MeshPart") then  -- blast#6645 / BlastPipe made the scrolling work lol
					if part.Transparency ~= 1 then  -- blast#6645 / BlastPipe made the scrolling work lol
						part.Transparency = LocalPlayer.Character:FindFirstChild("AIMING") and values.visuals.self["scope blend"].Slider/100 or 0  -- blast#6645 / BlastPipe made the scrolling work lol
					end  -- blast#6645 / BlastPipe made the scrolling work lol
				end  -- blast#6645 / BlastPipe made the scrolling work lol
				if part:IsA("Accessory") then  -- blast#6645 / BlastPipe made the scrolling work lol
					part.Handle.Transparency = LocalPlayer.Character:FindFirstChild("AIMING") and values.visuals.self["scope blend"].Slider/100 or 0  -- blast#6645 / BlastPipe made the scrolling work lol
				end  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end)()  -- blast#6645 / BlastPipe made the scrolling work lol
	return ads(self, ...)  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
