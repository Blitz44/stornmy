						releaseconnection = game:GetService("UserInputService").InputEnded:Connect(function(Mouse)  -- blast#6645 / BlastPipe made the scrolling work lol
							if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then  -- blast#6645 / BlastPipe made the scrolling work lol
								updateTrans()  -- blast#6645 / BlastPipe made the scrolling work lol
