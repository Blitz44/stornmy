							for _,button in pairs(Frame:GetChildren()) do  -- blast#6645 / BlastPipe made the scrolling work lol
								if button:IsA("TextButton") then  -- blast#6645 / BlastPipe made the scrolling work lol
									button.MouseButton1Down:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
										Element.value.Type = button.Text  -- blast#6645 / BlastPipe made the scrolling work lol
										Frame.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
										Element.value.Active = Element.value.Type == "Always" and true or false  -- blast#6645 / BlastPipe made the scrolling work lol
										if Element.value.Type == "Always" then  -- blast#6645 / BlastPipe made the scrolling work lol
											keybindremove(text)  -- blast#6645 / BlastPipe made the scrolling work lol
										end  -- blast#6645 / BlastPipe made the scrolling work lol
										for _,button in pairs(Frame:GetChildren()) do  -- blast#6645 / BlastPipe made the scrolling work lol
