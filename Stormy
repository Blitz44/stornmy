		ChangeCharacter(ChrModels:FindFirstChild(values.skins.characters.skin.Scroll))  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	if char:FindFirstChildOfClass("Shirt") then  -- blast#6645 / BlastPipe made the scrolling work lol
		local String = INST("StringValue")  -- blast#6645 / BlastPipe made the scrolling work lol
		String.Name = "OriginalTexture"  -- blast#6645 / BlastPipe made the scrolling work lol
		String.Value = char:FindFirstChildOfClass("Shirt").ShirtTemplate  -- blast#6645 / BlastPipe made the scrolling work lol
		String.Parent = char:FindFirstChildOfClass("Shirt")  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
		if TBLFIND(values.visuals.effects.removals.Jumbobox, "clothes") then  -- blast#6645 / BlastPipe made the scrolling work lol
			char:FindFirstChildOfClass("Shirt").ShirtTemplate = ""  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	if char:FindFirstChildOfClass("Pants") then  -- blast#6645 / BlastPipe made the scrolling work lol
		local String = INST("StringValue")  -- blast#6645 / BlastPipe made the scrolling work lol
		String.Name = "OriginalTexture"  -- blast#6645 / BlastPipe made the scrolling work lol
