	function keybindadd(text)  -- blast#6645 / BlastPipe made the scrolling work lol
		if not KeybindList.TextLabel.Frame:FindFirstChild(text) then  -- blast#6645 / BlastPipe made the scrolling work lol
			local TextLabel = INST("TextLabel")  -- blast#6645 / BlastPipe made the scrolling work lol
			TextLabel.BackgroundColor3 = COL3RGB(30, 30, 39)  -- blast#6645 / BlastPipe made the scrolling work lol
			TextLabel.BorderColor3 = COL3RGB(255, 37, 110)  -- blast#6645 / BlastPipe made the scrolling work lol
			TextLabel.BorderSizePixel = 0  -- blast#6645 / BlastPipe made the scrolling work lol
			TextLabel.Size = UDIM2(0, 155, 0, 24)  -- blast#6645 / BlastPipe made the scrolling work lol
			TextLabel.ZIndex = 2  -- blast#6645 / BlastPipe made the scrolling work lol
			TextLabel.Font = Enum.Font.SourceSansSemibold  -- blast#6645 / BlastPipe made the scrolling work lol
			TextLabel.Text = text  -- blast#6645 / BlastPipe made the scrolling work lol
			TextLabel.TextColor3 = COL3RGB(255, 255, 255)  -- blast#6645 / BlastPipe made the scrolling work lol
			TextLabel.TextSize = 14.000  -- blast#6645 / BlastPipe made the scrolling work lol
			TextLabel.Name = text  -- blast#6645 / BlastPipe made the scrolling work lol
			TextLabel.Parent = KeybindList.TextLabel.Frame  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
	function keybindremove(text)  -- blast#6645 / BlastPipe made the scrolling work lol
		if KeybindList.TextLabel.Frame:FindFirstChild(text) then  -- blast#6645 / BlastPipe made the scrolling work lol
			KeybindList.TextLabel.Frame:FindFirstChild(text):Destroy()  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
	function library:SetKeybindVisible(Joe)  -- blast#6645 / BlastPipe made the scrolling work lol
		KeybindList.Enabled = Joe  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
	library.dragging = false  -- blast#6645 / BlastPipe made the scrolling work lol
	do  -- blast#6645 / BlastPipe made the scrolling work lol
		local UserInputService = game:GetService("UserInputService")  -- blast#6645 / BlastPipe made the scrolling work lol
		local a = Menu  -- blast#6645 / BlastPipe made the scrolling work lol
		local dragInput  -- blast#6645 / BlastPipe made the scrolling work lol
		local dragStart  -- blast#6645 / BlastPipe made the scrolling work lol
		local startPos  -- blast#6645 / BlastPipe made the scrolling work lol
		local function update(input)  -- blast#6645 / BlastPipe made the scrolling work lol
			local delta = input.Position - dragStart  -- blast#6645 / BlastPipe made the scrolling work lol
			a.Position = UDIM2(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
		a.InputBegan:Connect(function(input)  -- blast#6645 / BlastPipe made the scrolling work lol
			if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then  -- blast#6645 / BlastPipe made the scrolling work lol
				library.dragging = true  -- blast#6645 / BlastPipe made the scrolling work lol
				dragStart = input.Position  -- blast#6645 / BlastPipe made the scrolling work lol
				startPos = a.Position  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
				input.Changed:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
					if input.UserInputState == Enum.UserInputState.End then  -- blast#6645 / BlastPipe made the scrolling work lol
						library.dragging = false  -- blast#6645 / BlastPipe made the scrolling work lol
					end  -- blast#6645 / BlastPipe made the scrolling work lol
				end)  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		end)  -- blast#6645 / BlastPipe made the scrolling work lol
		a.InputChanged:Connect(function(input)  -- blast#6645 / BlastPipe made the scrolling work lol
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then  -- blast#6645 / BlastPipe made the scrolling work lol
				dragInput = input  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		end)  -- blast#6645 / BlastPipe made the scrolling work lol
		UserInputService.InputChanged:Connect(function(input)  -- blast#6645 / BlastPipe made the scrolling work lol
			if input == dragInput and library.dragging then  -- blast#6645 / BlastPipe made the scrolling work lol
				update(input)  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		end)  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
