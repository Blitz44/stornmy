local function GetWeaponRage(weapon)  -- blast#6645 / BlastPipe made the scrolling work lol
	return TBLFIND(weps.Pistol, weapon) and "pistol" or TBLFIND(weps.Rifle, weapon) and "rifle" or weapon == "AWP" and "awp" or weapon == "G3SG1"  and "auto" or weapon == "Scout" and "scout" or "default"  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
local function GetStatsRage(weapon)  -- blast#6645 / BlastPipe made the scrolling work lol
	if weapon == "default" then  -- blast#6645 / BlastPipe made the scrolling work lol
		return values.rage.weapons.default  -- blast#6645 / BlastPipe made the scrolling work lol
	else  -- blast#6645 / BlastPipe made the scrolling work lol
		if values.rage.weapons[weapon]["override default"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
			return values.rage.weapons[weapon]  -- blast#6645 / BlastPipe made the scrolling work lol
		else  -- blast#6645 / BlastPipe made the scrolling work lol
			return values.rage.weapons.default  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
local function GetWeaponLegit(weapon)  -- blast#6645 / BlastPipe made the scrolling work lol
	return TBLFIND(weps2.Pistol, weapon) and "pistol" or TBLFIND(weps2.Rifle, weapon) and "rifle" or TBLFIND(weps2.SMG, weapon) and "smg" or TBLFIND(weps2.Sniper, weapon) and "sniper" or "default"  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
local function GetStatsLegit(weapon)  -- blast#6645 / BlastPipe made the scrolling work lol
	if weapon == "default" then  -- blast#6645 / BlastPipe made the scrolling work lol
		return values.legit.main.default  -- blast#6645 / BlastPipe made the scrolling work lol
	else  -- blast#6645 / BlastPipe made the scrolling work lol
		if values.legit.main[weapon]["override default"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
			return values.legit.main[weapon]  -- blast#6645 / BlastPipe made the scrolling work lol
		else  -- blast#6645 / BlastPipe made the scrolling work lol
			return values.legit.main.default  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
