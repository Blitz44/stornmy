function rgbtotbl(rgb)  -- blast#6645 / BlastPipe made the scrolling work lol
	return {R = rgb.R, G = rgb.G, B = rgb.B}  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
function tbltorgb(tbl)  -- blast#6645 / BlastPipe made the scrolling work lol
	return COL3(tbl.R, tbl.G, tbl.B)  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
local function deepCopy(original)  -- blast#6645 / BlastPipe made the scrolling work lol
	local copy = {}  -- blast#6645 / BlastPipe made the scrolling work lol
	for k, v in pairs(original) do  -- blast#6645 / BlastPipe made the scrolling work lol
		if type(v) == "table" then  -- blast#6645 / BlastPipe made the scrolling work lol
			v = deepCopy(v)  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
		copy[k] = v  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	return copy  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
function library:ConfigFix(cfg)  -- blast#6645 / BlastPipe made the scrolling work lol
	local copy = game:GetService("HttpService"):JSONDecode(readfile(cfglocation..cfg..".txt"))  -- blast#6645 / BlastPipe made the scrolling work lol
	for i,Tabs in pairs(copy) do  -- blast#6645 / BlastPipe made the scrolling work lol
		for i,Sectors in pairs(Tabs) do  -- blast#6645 / BlastPipe made the scrolling work lol
			for i,Elements in pairs(Sectors) do  -- blast#6645 / BlastPipe made the scrolling work lol
				if Elements.Color ~= nil then  -- blast#6645 / BlastPipe made the scrolling work lol
					local a = Elements.Color  -- blast#6645 / BlastPipe made the scrolling work lol
					Elements.Color = tbltorgb(a)  -- blast#6645 / BlastPipe made the scrolling work lol
				end  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	return copy  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
function library:SaveConfig(cfg)  -- blast#6645 / BlastPipe made the scrolling work lol
	local copy = deepCopy(values)  -- blast#6645 / BlastPipe made the scrolling work lol
	for i,Tabs in pairs(copy) do  -- blast#6645 / BlastPipe made the scrolling work lol
		for i,Sectors in pairs(Tabs) do  -- blast#6645 / BlastPipe made the scrolling work lol
			for i,Elements in pairs(Sectors) do  -- blast#6645 / BlastPipe made the scrolling work lol
				if Elements.Color ~= nil then  -- blast#6645 / BlastPipe made the scrolling work lol
					Elements.Color = {R=Elements.Color.R, G=Elements.Color.G, B=Elements.Color.B}  -- blast#6645 / BlastPipe made the scrolling work lol
				end  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	writefile(cfglocation..cfg..".txt", game:GetService("HttpService"):JSONEncode(copy))  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
