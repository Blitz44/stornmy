-- blast#6645 / BlastPipe made the scrolling work lol
						local function updateColor()  -- blast#6645 / BlastPipe made the scrolling work lol
							local ColorX = (CLAMP(mouse.X - Colorpick.AbsolutePosition.X, 0, Colorpick.AbsoluteSize.X)/Colorpick.AbsoluteSize.X)  -- blast#6645 / BlastPipe made the scrolling work lol
							local ColorY = (CLAMP(mouse.Y - Colorpick.AbsolutePosition.Y, 0, Colorpick.AbsoluteSize.Y)/Colorpick.AbsoluteSize.Y)  -- blast#6645 / BlastPipe made the scrolling work lol
							ColorDrag.Position = UDIM2(ColorX, 0, ColorY, 0)  -- blast#6645 / BlastPipe made the scrolling work lol
							ColorS = 1-ColorX  -- blast#6645 / BlastPipe made the scrolling work lol
							ColorV = 1-ColorY  -- blast#6645 / BlastPipe made the scrolling work lol
							Colorpick.ImageColor3 = COL3HSV(ColorH, 1, 1)  -- blast#6645 / BlastPipe made the scrolling work lol
							ColorP.BackgroundColor3 = COL3HSV(ColorH, ColorS, ColorV)  -- blast#6645 / BlastPipe made the scrolling work lol
							Transcolor.ImageColor3 = COL3HSV(ColorH, 1, 1)  -- blast#6645 / BlastPipe made the scrolling work lol
							values[tabname][sectorname][tabtext][text] = Element.value  -- blast#6645 / BlastPipe made the scrolling work lol
							Element.value.Color = COL3HSV(ColorH, ColorS, ColorV)  -- blast#6645 / BlastPipe made the scrolling work lol
							callback(Element.value)  -- blast#6645 / BlastPipe made the scrolling work lol
						end  -- blast#6645 / BlastPipe made the scrolling work lol
						local function updateHue()  -- blast#6645 / BlastPipe made the scrolling work lol
							local y = CLAMP(mouse.Y - Huepick.AbsolutePosition.Y, 0, 148)  -- blast#6645 / BlastPipe made the scrolling work lol
							Huedrag.Position = UDIM2(0, 0, 0, y)  -- blast#6645 / BlastPipe made the scrolling work lol
							hue = y/148  -- blast#6645 / BlastPipe made the scrolling work lol
							ColorH = 1-hue  -- blast#6645 / BlastPipe made the scrolling work lol
							Colorpick.ImageColor3 = COL3HSV(ColorH, 1, 1)  -- blast#6645 / BlastPipe made the scrolling work lol
							Transcolor.ImageColor3 = COL3HSV(ColorH, 1, 1)  -- blast#6645 / BlastPipe made the scrolling work lol
							ColorP.BackgroundColor3 = COL3HSV(ColorH, ColorS, ColorV)  -- blast#6645 / BlastPipe made the scrolling work lol
							values[tabname][sectorname][tabtext][text] = Element.value  -- blast#6645 / BlastPipe made the scrolling work lol
							Element.value.Color = COL3HSV(ColorH, ColorS, ColorV)  -- blast#6645 / BlastPipe made the scrolling work lol
							callback(Element.value)  -- blast#6645 / BlastPipe made the scrolling work lol
						end  -- blast#6645 / BlastPipe made the scrolling work lol
						local function updateTrans()  -- blast#6645 / BlastPipe made the scrolling work lol
							local x = CLAMP(mouse.X - Transpick.AbsolutePosition.X, 0, 178)  -- blast#6645 / BlastPipe made the scrolling work lol
							Transdrag.Position = UDIM2(0, x, 0, 0)  -- blast#6645 / BlastPipe made the scrolling work lol
							Element.value.Transparency = (x/178)  -- blast#6645 / BlastPipe made the scrolling work lol
							values[tabname][sectorname][tabtext][text] = Element.value  -- blast#6645 / BlastPipe made the scrolling work lol
							callback(Element.value)  -- blast#6645 / BlastPipe made the scrolling work lol
						end  -- blast#6645 / BlastPipe made the scrolling work lol
						Transpick.MouseButton1Down:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
							updateTrans()  -- blast#6645 / BlastPipe made the scrolling work lol
							moveconnection = mouse.Move:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
								updateTrans()  -- blast#6645 / BlastPipe made the scrolling work lol
							end)  -- blast#6645 / BlastPipe made the scrolling work lol
							releaseconnection = game:GetService("UserInputService").InputEnded:Connect(function(Mouse)  -- blast#6645 / BlastPipe made the scrolling work lol
								if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then  -- blast#6645 / BlastPipe made the scrolling work lol
									updateTrans()  -- blast#6645 / BlastPipe made the scrolling work lol
									moveconnection:Disconnect()  -- blast#6645 / BlastPipe made the scrolling work lol
									releaseconnection:Disconnect()  -- blast#6645 / BlastPipe made the scrolling work lol
								end  -- blast#6645 / BlastPipe made the scrolling work lol
							end)  -- blast#6645 / BlastPipe made the scrolling work lol
						end)  -- blast#6645 / BlastPipe made the scrolling work lol
						Colorpick.MouseButton1Down:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
							updateColor()  -- blast#6645 / BlastPipe made the scrolling work lol
							moveconnection = mouse.Move:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
								updateColor()  -- blast#6645 / BlastPipe made the scrolling work lol
							end)  -- blast#6645 / BlastPipe made the scrolling work lol
							releaseconnection = game:GetService("UserInputService").InputEnded:Connect(function(Mouse)  -- blast#6645 / BlastPipe made the scrolling work lol
								if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then  -- blast#6645 / BlastPipe made the scrolling work lol
