Players.PlayerAdded:Connect(function(Player)  -- blast#6645 / BlastPipe made the scrolling work lol
	Player:GetPropertyChangedSignal("Team"):Connect(function(new)  -- blast#6645 / BlastPipe made the scrolling work lol
		wait()  -- blast#6645 / BlastPipe made the scrolling work lol
		if Player.Character and Player.Character:FindFirstChild("HumanoidRootPart") then  -- blast#6645 / BlastPipe made the scrolling work lol
			for _2,Obj in pairs(Player.Character:GetDescendants()) do  -- blast#6645 / BlastPipe made the scrolling work lol
				if Obj.Name == "VisibleCham" or Obj.Name == "WallCham" then  -- blast#6645 / BlastPipe made the scrolling work lol
					if values.visuals.players.chams.Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
						if values.visuals.players.teammates.Toggle or Player.Team ~= LocalPlayer.Team then  -- blast#6645 / BlastPipe made the scrolling work lol
							Obj.Visible = true  -- blast#6645 / BlastPipe made the scrolling work lol
						else  -- blast#6645 / BlastPipe made the scrolling work lol
							Obj.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
						end  -- blast#6645 / BlastPipe made the scrolling work lol
					else  -- blast#6645 / BlastPipe made the scrolling work lol
						Obj.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
					end  -- blast#6645 / BlastPipe made the scrolling work lol
					Obj.Color3 = values.visuals.players.chams.Color  -- blast#6645 / BlastPipe made the scrolling work lol
				end  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end)  -- blast#6645 / BlastPipe made the scrolling work lol
	Player.CharacterAdded:Connect(function(Character)  -- blast#6645 / BlastPipe made the scrolling work lol
		Character.ChildAdded:Connect(function(obj)  -- blast#6645 / BlastPipe made the scrolling work lol
			wait(1)  -- blast#6645 / BlastPipe made the scrolling work lol
			CollisionTBL(obj)  -- blast#6645 / BlastPipe made the scrolling work lol
		end)  -- blast#6645 / BlastPipe made the scrolling work lol
		wait(1)  -- blast#6645 / BlastPipe made the scrolling work lol
		if Character ~= nil then  -- blast#6645 / BlastPipe made the scrolling work lol
			local Value = INST("Vector3Value")  -- blast#6645 / BlastPipe made the scrolling work lol
			Value.Name = "OldPosition"  -- blast#6645 / BlastPipe made the scrolling work lol
			Value.Value = Character.HumanoidRootPart.Position  -- blast#6645 / BlastPipe made the scrolling work lol
