														Client.firebullet()  -- blast#6645 / BlastPipe made the scrolling work lol
														if values.rage.exploits["double tap"].Toggle and values.rage.exploits["double tap"].Active then  -- blast#6645 / BlastPipe made the scrolling work lol
															Client.firebullet()  -- blast#6645 / BlastPipe made the scrolling work lol
														end  -- blast#6645 / BlastPipe made the scrolling work lol
													elseif values.rage.aimbot["automatic fire"].Dropdown == "hitpart" then  -- blast#6645 / BlastPipe made the scrolling work lol
														Client.firebullet()  -- blast#6645 / BlastPipe made the scrolling work lol
														local Arguments = {  -- blast#6645 / BlastPipe made the scrolling work lol
															[1] = EndHit,  -- blast#6645 / BlastPipe made the scrolling work lol
															[2] = EndHit.Position,  -- blast#6645 / BlastPipe made the scrolling work lol
															[3] = LocalPlayer.Character.EquippedTool.Value,  -- blast#6645 / BlastPipe made the scrolling work lol
															[4] = 100,  -- blast#6645 / BlastPipe made the scrolling work lol
															[5] = LocalPlayer.Character.Gun,  -- blast#6645 / BlastPipe made the scrolling work lol
															[8] = 1,  -- blast#6645 / BlastPipe made the scrolling work lol
															[9] = false,  -- blast#6645 / BlastPipe made the scrolling work lol
