						function Element:SetValue(value)  -- blast#6645 / BlastPipe made the scrolling work lol
							Element.value = value  -- blast#6645 / BlastPipe made the scrolling work lol
							local a  -- blast#6645 / BlastPipe made the scrolling work lol
							if min > 0 then  -- blast#6645 / BlastPipe made the scrolling work lol
								a = ((Element.value.Slider - min)) / (max-min)  -- blast#6645 / BlastPipe made the scrolling work lol
							else  -- blast#6645 / BlastPipe made the scrolling work lol
								a = (Element.value.Slider-min)/(max-min)  -- blast#6645 / BlastPipe made the scrolling work lol
							end  -- blast#6645 / BlastPipe made the scrolling work lol
							Value.Text = Element.value.Slider  -- blast#6645 / BlastPipe made the scrolling work lol
							Frame.Size = UDIM2(a,0,1,0)  -- blast#6645 / BlastPipe made the scrolling work lol
							values[tabname][sectorname][tabtext][text] = Element.value  -- blast#6645 / BlastPipe made the scrolling work lol
							callback(value)  -- blast#6645 / BlastPipe made the scrolling work lol
						end  -- blast#6645 / BlastPipe made the scrolling work lol
						local a  -- blast#6645 / BlastPipe made the scrolling work lol
						if min > 0 then  -- blast#6645 / BlastPipe made the scrolling work lol
							a = ((Element.value.Slider - min)) / (max-min)  -- blast#6645 / BlastPipe made the scrolling work lol
