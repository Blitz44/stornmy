							callback(Element.value)  -- blast#6645 / BlastPipe made the scrolling work lol
							moveconnection = mouse.Move:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
								Frame.Size = UDIM2(0, CLAMP(mouse.X - Frame.AbsolutePosition.X, 0, 175), 0, 5)  -- blast#6645 / BlastPipe made the scrolling work lol
								val = FLOOR((((tonumber(max) - tonumber(min)) / 175) * Frame.AbsoluteSize.X) + tonumber(min))  -- blast#6645 / BlastPipe made the scrolling work lol
								Value.Text = val  -- blast#6645 / BlastPipe made the scrolling work lol
