	Tab:Element("Dropdown", "hitbox", {options = {"closest", "head", "chest"}})  -- blast#6645 / BlastPipe made the scrolling work lol
	Tab:Element("Slider", "field of view", {min = 30, max = 420, default = 120})  -- blast#6645 / BlastPipe made the scrolling work lol
	Tab:Element("Slider", "smoothing", {min = 1, max = 50, default = 1})  -- blast#6645 / BlastPipe made the scrolling work lol
	Tab:Element("Toggle", "silent aim")  -- blast#6645 / BlastPipe made the scrolling work lol
	Tab:Element("Slider", "hitchance", {min = 1, max = 100, default = 100})  -- blast#6645 / BlastPipe made the scrolling work lol
	Tab:Element("Dropdown", "priority", {options = {"closest", "head", "chest"}})  -- blast#6645 / BlastPipe made the scrolling work lol
	Tab:Element("Toggle", "triggerbot")  -- blast#6645 / BlastPipe made the scrolling work lol
	Tab:Element("Slider", "delay (ms)", {min = 0, max = 300, default = 200})  -- blast#6645 / BlastPipe made the scrolling work lol
	Tab:Element("Slider", "minimum dmg", {min = 0, max = 100, default = 15})  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
