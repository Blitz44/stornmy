						local Player = game.Players.LocalPlayer  -- blast#6645 / BlastPipe made the scrolling work lol
						local Mouse = Player:GetMouse()  -- blast#6645 / BlastPipe made the scrolling work lol
						local InFrame = false  -- blast#6645 / BlastPipe made the scrolling work lol
						Frame.MouseEnter:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
							InFrame = true  -- blast#6645 / BlastPipe made the scrolling work lol
						end)  -- blast#6645 / BlastPipe made the scrolling work lol
						Frame.MouseLeave:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
							InFrame = false  -- blast#6645 / BlastPipe made the scrolling work lol
						end)  -- blast#6645 / BlastPipe made the scrolling work lol
						local InFrame2 = false  -- blast#6645 / BlastPipe made the scrolling work lol
