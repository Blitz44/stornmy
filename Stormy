				tbl.Box.Visible = OnScreen  -- blast#6645 / BlastPipe made the scrolling work lol
				if Drawings then  -- blast#6645 / BlastPipe made the scrolling work lol
					tbl.BoxOutline.Size = tbl.Box.Size  -- blast#6645 / BlastPipe made the scrolling work lol
					tbl.BoxOutline.Position = tbl.Box.Position  -- blast#6645 / BlastPipe made the scrolling work lol
					tbl.BoxOutline.Visible = OnScreen  -- blast#6645 / BlastPipe made the scrolling work lol
				else  -- blast#6645 / BlastPipe made the scrolling work lol
					tbl.BoxOutline.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
				end  -- blast#6645 / BlastPipe made the scrolling work lol
			else  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Box.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.BoxOutline.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
			if values.visuals.players.health.Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Health.Color = COL3(0,1,0)  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Health.From = Vec2((tbl.Box.Position.X - 5), tbl.Box.Position.Y + tbl.Box.Size.Y)  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Health.To = Vec2(tbl.Health.From.X, tbl.Health.From.Y - CLAMP(Player.Character.Humanoid.Health / Player.Character.Humanoid.MaxHealth, 0, 1) * tbl.Box.Size.Y)  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Health.Visible = OnScreen  -- blast#6645 / BlastPipe made the scrolling work lol
				if Drawings then  -- blast#6645 / BlastPipe made the scrolling work lol
					tbl.HealthOutline.From = Vec2(tbl.Health.From.X, tbl.Box.Position.Y + tbl.Box.Size.Y + 1)  -- blast#6645 / BlastPipe made the scrolling work lol
					tbl.HealthOutline.To = Vec2(tbl.Health.From.X, (tbl.Health.From.Y - 1 * tbl.Box.Size.Y) -1)  -- blast#6645 / BlastPipe made the scrolling work lol
					tbl.HealthOutline.Visible = OnScreen  -- blast#6645 / BlastPipe made the scrolling work lol
				else  -- blast#6645 / BlastPipe made the scrolling work lol
					tbl.HealthOutline.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
				end  -- blast#6645 / BlastPipe made the scrolling work lol
			else  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Health.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.HealthOutline.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
			if values.visuals.players.weapon.Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Weapon.Color = values.visuals.players.weapon.Color  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Weapon.Text = Player.Character.EquippedTool.Value  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Weapon.Position = Vec2(tbl.Box.Size.X/2 + tbl.Box.Position.X, tbl.Box.Size.Y + tbl.Box.Position.Y + 1)  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Weapon.Font = Drawing.Fonts[values.visuals.players.font.Dropdown]  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Weapon.Outline = Text  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Weapon.Size = values.visuals.players.size.Slider  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Weapon.Visible = OnScreen  -- blast#6645 / BlastPipe made the scrolling work lol
			else  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Weapon.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
			if values.visuals.players["weapon icon"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
				Items[Player.Name].ImageColor3 = values.visuals.players["weapon icon"].Color  -- blast#6645 / BlastPipe made the scrolling work lol
				Items[Player.Name].Image = GetIcon.getWeaponOfKiller(Player.Character.EquippedTool.Value)  -- blast#6645 / BlastPipe made the scrolling work lol
				Items[Player.Name].Position = UDIM2(0, tbl.Box.Size.X/2 + tbl.Box.Position.X, 0, tbl.Box.Size.Y + tbl.Box.Position.Y + (values.visuals.players.weapon.Toggle and -10 or -22))  -- blast#6645 / BlastPipe made the scrolling work lol
				Items[Player.Name].Visible = OnScreen  -- blast#6645 / BlastPipe made the scrolling work lol
			else  -- blast#6645 / BlastPipe made the scrolling work lol
				Items[Player.Name].Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
			if values.visuals.players.name.Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Name.Color = values.visuals.players.name.Color  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Name.Text = Player.Name  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Name.Position = Vec2(tbl.Box.Size.X/2 + tbl.Box.Position.X,  tbl.Box.Position.Y - 16)  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Name.Font = Drawing.Fonts[values.visuals.players.font.Dropdown]  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Name.Outline = Text  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Name.Size = values.visuals.players.size.Slider  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Name.Visible = OnScreen  -- blast#6645 / BlastPipe made the scrolling work lol
			else  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Name.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
			local LastInfoPos = tbl.Box.Position.Y - 1  -- blast#6645 / BlastPipe made the scrolling work lol
			if TBLFIND(values.visuals.players.indicators.Jumbobox, "armor") and Player:FindFirstChild("Kevlar") then  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Armor.Color = COL3RGB(0, 150, 255)  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Armor.Text = Player:FindFirstChild("Helmet") and "HK" or "K"  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Armor.Position = Vec2(tbl.Box.Size.X + tbl.Box.Position.X + 12, LastInfoPos)  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Armor.Font = Drawing.Fonts[values.visuals.players.font.Dropdown]  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Armor.Outline = Text  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Armor.Size = values.visuals.players.size.Slider  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Armor.Visible = OnScreen  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
				LastInfoPos = LastInfoPos + values.visuals.players.size.Slider  -- blast#6645 / BlastPipe made the scrolling work lol
			else  -- blast#6645 / BlastPipe made the scrolling work lol
				tbl.Armor.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		else  -- blast#6645 / BlastPipe made the scrolling work lol
			if Player.Name ~= LocalPlayer.Name then  -- blast#6645 / BlastPipe made the scrolling work lol
				Items[Player.Name].Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
				for i,v in pairs(tbl) do  -- blast#6645 / BlastPipe made the scrolling work lol
					v.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
				end  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
	if (values.misc.ui.scaling.Toggle) then  -- blast#6645 / BlastPipe made the scrolling work lol
		gui:SetScale(values.misc.ui.amount.Slider / 10)  -- blast#6645 / BlastPipe made the scrolling work lol
	else  -- blast#6645 / BlastPipe made the scrolling work lol
		gui:SetScale(1)  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
end)  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
local mt = getrawmetatable(game)  -- blast#6645 / BlastPipe made the scrolling work lol
local oldNamecall = mt.__namecall  -- blast#6645 / BlastPipe made the scrolling work lol
local oldIndex = mt.__index  -- blast#6645 / BlastPipe made the scrolling work lol
local oldNewIndex = mt.__newindex  -- blast#6645 / BlastPipe made the scrolling work lol
setreadonly(mt,false)  -- blast#6645 / BlastPipe made the scrolling work lol
mt.__namecall = function(self, ...)  -- blast#6645 / BlastPipe made the scrolling work lol
	local method = tostring(getnamecallmethod())  -- blast#6645 / BlastPipe made the scrolling work lol
	local args = {...}  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
	if method == "SetPrimaryPartCFrame" and self.Name == "Arms" then  -- blast#6645 / BlastPipe made the scrolling work lol
		if values.visuals.self["third person"].Toggle and values.visuals.self["third person"].Active and LocalPlayer.Character then  -- blast#6645 / BlastPipe made the scrolling work lol
			args[1] = args[1] * CF(99, 99, 99)  -- blast#6645 / BlastPipe made the scrolling work lol
		else  -- blast#6645 / BlastPipe made the scrolling work lol
			if values.visuals.self["viewmodel changer"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
				args[1] = args[1] * ViewmodelOffset  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	if method == "SetPrimaryPartCFrame" and self.Name ~= "Arms" then  -- blast#6645 / BlastPipe made the scrolling work lol
		args[1] = args[1] + Vec3(0, 3, 0)  -- blast#6645 / BlastPipe made the scrolling work lol
		coroutine.wrap(function()  -- blast#6645 / BlastPipe made the scrolling work lol
			DisableAA = true  -- blast#6645 / BlastPipe made the scrolling work lol
			wait(2)  -- blast#6645 / BlastPipe made the scrolling work lol
			DisableAA = false  -- blast#6645 / BlastPipe made the scrolling work lol
		end)()  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	if method == "Kick" then  -- blast#6645 / BlastPipe made the scrolling work lol
		return  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	if method == "FireServer" then  -- blast#6645 / BlastPipe made the scrolling work lol
		if LEN(self.Name) == 38 then  -- blast#6645 / BlastPipe made the scrolling work lol
			return  -- blast#6645 / BlastPipe made the scrolling work lol
		elseif self.Name == "FallDamage" and TBLFIND(values.misc.client["damage bypass"].Jumbobox, "fall") or values.misc.movement["jump bug"].Toggle and values.misc.movement["jump bug"].Active then  -- blast#6645 / BlastPipe made the scrolling work lol
			return  -- blast#6645 / BlastPipe made the scrolling work lol
		elseif self.Name == "BURNME" and TBLFIND(values.misc.client["damage bypass"].Jumbobox, "fire") then  -- blast#6645 / BlastPipe made the scrolling work lol
			return  -- blast#6645 / BlastPipe made the scrolling work lol
		elseif self.Name == "ControlTurn" and not checkcaller() then  -- blast#6645 / BlastPipe made the scrolling work lol
			return  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
		if self.Name == "PlayerChatted" and values.misc.client["chat alive"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
			args[2] = false  -- blast#6645 / BlastPipe made the scrolling work lol
			args[3] = "Innocent"  -- blast#6645 / BlastPipe made the scrolling work lol
			args[4] = false  -- blast#6645 / BlastPipe made the scrolling work lol
			args[5] = false  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
		if self.Name == "ReplicateCamera" and values.misc.client["anti spectate"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
			args[1] = CF()  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	if method == "FindPartOnRayWithWhitelist" and not checkcaller() and Client.gun ~= "none" and Client.gun.Name ~= "C4" then  -- blast#6645 / BlastPipe made the scrolling work lol
		if #args[2] == 1 and args[2][1].Name == "SpawnPoints" then  -- blast#6645 / BlastPipe made the scrolling work lol
			local Team = LocalPlayer.Status.Team.Value  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
			if TBLFIND(values.misc.client.shop.Jumbobox, "anywhere") then  -- blast#6645 / BlastPipe made the scrolling work lol
				return Team == "T" and args[2][1].BuyArea or args[2][1].BuyArea2  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	if method == "FindPartOnRayWithIgnoreList" and args[2][1] == workspace.Debris then  -- blast#6645 / BlastPipe made the scrolling work lol
		if not checkcaller() or Filter then  -- blast#6645 / BlastPipe made the scrolling work lol
			if TBLFIND(values.misc.client["gun modifiers"].Jumbobox, "penetration") then  -- blast#6645 / BlastPipe made the scrolling work lol
				INSERT(args[2], workspace.Map)  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
			if TBLFIND(values.misc.client["gun modifiers"].Jumbobox, "spread") then  -- blast#6645 / BlastPipe made the scrolling work lol
				args[1] = RAY(Camera.CFrame.p, Camera.CFrame.LookVector * Client.gun.Range.Value)  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
			local Stats = GetStatsLegit(GetWeaponLegit(Client.gun.Name))  -- blast#6645 / BlastPipe made the scrolling work lol
			if values.legit.aimbot["silent aim"].Toggle and values.legit.aimbot["silent aim"].Active and Stats["silent aim"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
				local Ignore = {LocalPlayer.Character, Camera, workspace.Map.Clips, workspace.Map.SpawnPoints, workspace.Debris}  -- blast#6645 / BlastPipe made the scrolling work lol
				local Closest = 9999  -- blast#6645 / BlastPipe made the scrolling work lol
				local Target  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
				if not TBLFIND(Stats.conditions.Jumbobox, "smoke") then  -- blast#6645 / BlastPipe made the scrolling work lol
					INSERT(Ignore, workspace.Ray_Ignore)  -- blast#6645 / BlastPipe made the scrolling work lol
				end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
				coroutine.wrap(function()  -- blast#6645 / BlastPipe made the scrolling work lol
					if not TBLFIND(Stats.conditions.Jumbobox, "blind") or LocalPlayer.PlayerGui.Blnd.Blind.BackgroundTransparency > 0.9 then  -- blast#6645 / BlastPipe made the scrolling work lol
						if not TBLFIND(Stats.conditions.Jumbobox, "blind") or SelfVelocity.Magnitude < 3 then  -- blast#6645 / BlastPipe made the scrolling work lol
							for _,Player in pairs(Players:GetPlayers()) do  -- blast#6645 / BlastPipe made the scrolling work lol
								if Player.Character and Player.Character:FindFirstChild("Humanoid") and Player.Character:FindFirstChild("Humanoid").Health > 0 then  -- blast#6645 / BlastPipe made the scrolling work lol
									if not values.legit.settings["forcefield check"].Toggle or not Player.Character:FindFirstChildOfClass("ForceField") then  -- blast#6645 / BlastPipe made the scrolling work lol
										if Player.Team ~= LocalPlayer.Team or values.legit.settings["free for all"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
											local Pos, onScreen = Camera:WorldToViewportPoint(Player.Character.HumanoidRootPart.Position)  -- blast#6645 / BlastPipe made the scrolling work lol
											if onScreen then  -- blast#6645 / BlastPipe made the scrolling work lol
												local Magnitude = (Vec2(Pos.X, Pos.Y) - Vec2(Mouse.X, Mouse.Y)).Magnitude  -- blast#6645 / BlastPipe made the scrolling work lol
												if Magnitude < Stats["field of view"].Slider then  -- blast#6645 / BlastPipe made the scrolling work lol
													local Hitbox = Stats.priority.Dropdown == "head" and Player.Character.Head or Stats.priority.Dropdown == "chest" and Player.Character.UpperTorso  -- blast#6645 / BlastPipe made the scrolling work lol
													if Stats.priority.Dropdown == "closest" then  -- blast#6645 / BlastPipe made the scrolling work lol
														local HeadPos = Camera:WorldToViewportPoint(Player.Character.Head.Position)  -- blast#6645 / BlastPipe made the scrolling work lol
														local TorsoPos = Camera:WorldToViewportPoint(Player.Character.UpperTorso.Position)  -- blast#6645 / BlastPipe made the scrolling work lol
														local HeadDistance = (Vec2(HeadPos.X, HeadPos.Y) - Vec2(Mouse.X, Mouse.Y)).Magnitude  -- blast#6645 / BlastPipe made the scrolling work lol
														local TorsoDistance = (Vec2(TorsoPos.X, TorsoPos.Y) - Vec2(Mouse.X, Mouse.Y)).Magnitude  -- blast#6645 / BlastPipe made the scrolling work lol
														if HeadDistance < TorsoDistance then  -- blast#6645 / BlastPipe made the scrolling work lol
															Hitbox = Player.Character.Head  -- blast#6645 / BlastPipe made the scrolling work lol
														else  -- blast#6645 / BlastPipe made the scrolling work lol
															Hitbox = Player.Character.UpperTorso  -- blast#6645 / BlastPipe made the scrolling work lol
														end  -- blast#6645 / BlastPipe made the scrolling work lol
													end  -- blast#6645 / BlastPipe made the scrolling work lol
													if Hitbox ~= nil then  -- blast#6645 / BlastPipe made the scrolling work lol
														if not TBLFIND(Stats.conditions.Jumbobox, "visible") then  -- blast#6645 / BlastPipe made the scrolling work lol
															Target = Hitbox  -- blast#6645 / BlastPipe made the scrolling work lol
														else  -- blast#6645 / BlastPipe made the scrolling work lol
															local Ray1 = RAY(Camera.CFrame.Position, (Hitbox.Position - Camera.CFrame.Position).unit * (Hitbox.Position - Camera.CFrame.Position).magnitude)  -- blast#6645 / BlastPipe made the scrolling work lol
															local Hit, Pos = workspace:FindPartOnRayWithIgnoreList(Ray1, Ignore, false, true)  -- blast#6645 / BlastPipe made the scrolling work lol
															if Hit and Hit:FindFirstAncestor(Player.Name) then  -- blast#6645 / BlastPipe made the scrolling work lol
																Target = Hitbox  -- blast#6645 / BlastPipe made the scrolling work lol
															end  -- blast#6645 / BlastPipe made the scrolling work lol
														end  -- blast#6645 / BlastPipe made the scrolling work lol
													end  -- blast#6645 / BlastPipe made the scrolling work lol
												end  -- blast#6645 / BlastPipe made the scrolling work lol
											end  -- blast#6645 / BlastPipe made the scrolling work lol
										end  -- blast#6645 / BlastPipe made the scrolling work lol
									end  -- blast#6645 / BlastPipe made the scrolling work lol
								end  -- blast#6645 / BlastPipe made the scrolling work lol
							end  -- blast#6645 / BlastPipe made the scrolling work lol
						end  -- blast#6645 / BlastPipe made the scrolling work lol
					end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
					local Hit = RANDOM(1, 100) <= Stats.hitchance.Slider  -- blast#6645 / BlastPipe made the scrolling work lol
					if Target ~= nil and Hit then  -- blast#6645 / BlastPipe made the scrolling work lol
						args[1] = RAY(Camera.CFrame.Position, (Target.Position - Camera.CFrame.Position).unit * (Target.Position - Camera.CFrame.Position).magnitude)  -- blast#6645 / BlastPipe made the scrolling work lol
					end  -- blast#6645 / BlastPipe made the scrolling work lol
				end)()  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
			if RageTarget ~= nil then  -- blast#6645 / BlastPipe made the scrolling work lol
				local Origin = values.rage.aimbot.origin.Dropdown == "character" and LocalPlayer.Character.LowerTorso.Position + Vec3(0, 2.5, 0) or Camera.CFrame.p  -- blast#6645 / BlastPipe made the scrolling work lol
				if values.rage.aimbot["delay shot"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
					spawn(function()  -- blast#6645 / BlastPipe made the scrolling work lol
						args[1] = RAY(Origin, (RageTarget.Position - Origin).unit * (RageTarget.Position - Origin).magnitude)  -- blast#6645 / BlastPipe made the scrolling work lol
					end)  -- blast#6645 / BlastPipe made the scrolling work lol
				else  -- blast#6645 / BlastPipe made the scrolling work lol
					args[1] = RAY(Origin, (RageTarget.Position - Origin).unit * (RageTarget.Position - Origin).magnitude)  -- blast#6645 / BlastPipe made the scrolling work lol
				end  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	if method == "InvokeServer" then  -- blast#6645 / BlastPipe made the scrolling work lol
		if self.Name == "Moolah" then  -- blast#6645 / BlastPipe made the scrolling work lol
			return  -- blast#6645 / BlastPipe made the scrolling work lol
		elseif self.Name == "Hugh" then  -- blast#6645 / BlastPipe made the scrolling work lol
			return  -- blast#6645 / BlastPipe made the scrolling work lol
		elseif self.Name == "Filter" and values.misc.chat["no filter"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
			return args[1]  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	if method == "LoadAnimation" and self.Name == "Humanoid" then  -- blast#6645 / BlastPipe made the scrolling work lol
		if values.rage.others["leg movement"].Dropdown == "slide" then  -- blast#6645 / BlastPipe made the scrolling work lol
			if FIND(args[1].Name, "Walk") or FIND(args[1].Name, "Run") then  -- blast#6645 / BlastPipe made the scrolling work lol
				args[1] = FakeAnim  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
		if values.rage.others["no animations"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
			args[1] = FakeAnim  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	if method == "FireServer" and self.Name == "HitPart" then  -- blast#6645 / BlastPipe made the scrolling work lol
		if values.rage.aimbot["force hit"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
			args[1] = RageTarget  -- blast#6645 / BlastPipe made the scrolling work lol
			args[2] = RageTarget.Position  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
		if (values.rage.aimbot["sex package"].Toggle and RageTarget ~= nil) then  -- blast#6645 / BlastPipe made the scrolling work lol
			coroutine.wrap(function()  -- blast#6645 / BlastPipe made the scrolling work lol
				if Players:GetPlayerFromCharacter(args[1].Parent) or args[1] == RageTarget then  -- blast#6645 / BlastPipe made the scrolling work lol
					local hrp = RageTarget.Parent.HumanoidRootPart.Position  -- blast#6645 / BlastPipe made the scrolling work lol
					local oldHrp = RageTarget.Parent.HumanoidRootPart.OldPosition.Value  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
					local vel = (Vec3(hrp.X, 0, hrp.Z) - Vec3(oldHrp.X, 0, oldHrp.Z)) / LastStep  -- blast#6645 / BlastPipe made the scrolling work lol
					local dir = Vec3(vel.X / vel.magnitude, 0, vel.Z / vel.magnitude)  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
					  -- blast#6645 / BlastPipe made the scrolling work lol
					args[2] = args[2] + dir * (Ping / (POW(Ping, 1.5)) * (dir / (dir / 2)))  -- blast#6645 / BlastPipe made the scrolling work lol
					args[4] = 0  -- blast#6645 / BlastPipe made the scrolling work lol
					args[12] = args[12] - 500  -- blast#6645 / BlastPipe made the scrolling work lol
				end  -- blast#6645 / BlastPipe made the scrolling work lol
			end)()  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
		  -- blast#6645 / BlastPipe made the scrolling work lol
		if values.visuals.world["bullet tracers"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
			coroutine.wrap(function()  -- blast#6645 / BlastPipe made the scrolling work lol
				local beam = INST("Part")  -- blast#6645 / BlastPipe made the scrolling work lol
				beam.Anchored = true  -- blast#6645 / BlastPipe made the scrolling work lol
				beam.CanCollide = false  -- blast#6645 / BlastPipe made the scrolling work lol
				beam.Material = Enum.Material.ForceField  -- blast#6645 / BlastPipe made the scrolling work lol
				beam.Color = values.visuals.world["bullet tracers"].Color  -- blast#6645 / BlastPipe made the scrolling work lol
				beam.Size = Vec3(0.1, 0.1, (Camera.CFrame.Position - args[2]).Magnitude)  -- blast#6645 / BlastPipe made the scrolling work lol
				beam.CFrame = CF(Camera.CFrame.Position, args[2]) * CF(0, 0, -beam.Size.Z / 2)  -- blast#6645 / BlastPipe made the scrolling work lol
				beam.Parent = workspace.Debris  -- blast#6645 / BlastPipe made the scrolling work lol
				library:Tween(beam, TweenInfo.new(1.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Transparency = 1})  -- blast#6645 / BlastPipe made the scrolling work lol
				wait(1.5)  -- blast#6645 / BlastPipe made the scrolling work lol
				beam:Destroy()  -- blast#6645 / BlastPipe made the scrolling work lol
			end)()  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
		if values.visuals.world["impacts"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
			coroutine.wrap(function()  -- blast#6645 / BlastPipe made the scrolling work lol
				local hit = INST("Part")  -- blast#6645 / BlastPipe made the scrolling work lol
				hit.Transparency = 1  -- blast#6645 / BlastPipe made the scrolling work lol
				hit.Anchored = true  -- blast#6645 / BlastPipe made the scrolling work lol
				hit.CanCollide = false  -- blast#6645 / BlastPipe made the scrolling work lol
				hit.Size = Vec3(0.3,0.3,0.3)  -- blast#6645 / BlastPipe made the scrolling work lol
				hit.Position = args[2]  -- blast#6645 / BlastPipe made the scrolling work lol
				local selection = INST("SelectionBox")  -- blast#6645 / BlastPipe made the scrolling work lol
				selection.LineThickness = 0  -- blast#6645 / BlastPipe made the scrolling work lol
				selection.SurfaceTransparency = 0.5  -- blast#6645 / BlastPipe made the scrolling work lol
				selection.Color3 = values.visuals.world["impacts"].Color  -- blast#6645 / BlastPipe made the scrolling work lol
				selection.SurfaceColor3 = values.visuals.world["impacts"].Color  -- blast#6645 / BlastPipe made the scrolling work lol
				selection.Parent = hit  -- blast#6645 / BlastPipe made the scrolling work lol
				selection.Adornee = hit  -- blast#6645 / BlastPipe made the scrolling work lol
				hit.Parent = workspace.Debris  -- blast#6645 / BlastPipe made the scrolling work lol
				wait(5.9)  -- blast#6645 / BlastPipe made the scrolling work lol
				library:Tween(selection, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {SurfaceTransparency = 1})  -- blast#6645 / BlastPipe made the scrolling work lol
				hit:Destroy()  -- blast#6645 / BlastPipe made the scrolling work lol
			end)()  -- blast#6645 / BlastPipe made the scrolling work lol
			if values.visuals.world["hit chams"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
				coroutine.wrap(function()  -- blast#6645 / BlastPipe made the scrolling work lol
					if Players:GetPlayerFromCharacter(args[1].Parent) and Players:GetPlayerFromCharacter(args[1].Parent).Team ~= LocalPlayer.Team then  -- blast#6645 / BlastPipe made the scrolling work lol
						for _,hitbox in pairs(args[1].Parent:GetChildren()) do  -- blast#6645 / BlastPipe made the scrolling work lol
							if hitbox:IsA("BasePart") or hitbox.Name == "Head" then  -- blast#6645 / BlastPipe made the scrolling work lol
								coroutine.wrap(function()  -- blast#6645 / BlastPipe made the scrolling work lol
									local part = INST("Part")  -- blast#6645 / BlastPipe made the scrolling work lol
									part.CFrame = hitbox.CFrame  -- blast#6645 / BlastPipe made the scrolling work lol
									part.Anchored = true  -- blast#6645 / BlastPipe made the scrolling work lol
									part.CanCollide = false  -- blast#6645 / BlastPipe made the scrolling work lol
									part.Material = Enum.Material.ForceField  -- blast#6645 / BlastPipe made the scrolling work lol
									part.Color = values.visuals.world["hit chams"].Color  -- blast#6645 / BlastPipe made the scrolling work lol
									part.Size = hitbox.Size  -- blast#6645 / BlastPipe made the scrolling work lol
									part.Parent = workspace.Debris  -- blast#6645 / BlastPipe made the scrolling work lol
									library:Tween(part, TweenInfo.new(2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Transparency = 1})  -- blast#6645 / BlastPipe made the scrolling work lol
									wait(2)  -- blast#6645 / BlastPipe made the scrolling work lol
									part:Destroy()  -- blast#6645 / BlastPipe made the scrolling work lol
								end)()  -- blast#6645 / BlastPipe made the scrolling work lol
							end  -- blast#6645 / BlastPipe made the scrolling work lol
						end  -- blast#6645 / BlastPipe made the scrolling work lol
					end  -- blast#6645 / BlastPipe made the scrolling work lol
				end)()  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
	return oldNamecall(self, unpack(args))  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
mt.__index = function(self, key)  -- blast#6645 / BlastPipe made the scrolling work lol
	local CallingScript = getcallingscript()  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
	if not checkcaller() and self == Viewmodels and LocalPlayer.Character ~= nil and LocalPlayer.Character:FindFirstChild("UpperTorso") then  -- blast#6645 / BlastPipe made the scrolling work lol
		local WeaponName = GSUB(key, "v_", "")  -- blast#6645 / BlastPipe made the scrolling work lol
		if not FIND(WeaponName, "Arms") then  -- blast#6645 / BlastPipe made the scrolling work lol
			if Weapons[WeaponName]:FindFirstChild("Melee") and values.skins.knife["knife changer"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
				if Viewmodels:FindFirstChild("v_"..values.skins.knife.model.Scroll) then  -- blast#6645 / BlastPipe made the scrolling work lol
					return Viewmodels:FindFirstChild("v_"..values.skins.knife.model.Scroll)  -- blast#6645 / BlastPipe made the scrolling work lol
				else  -- blast#6645 / BlastPipe made the scrolling work lol
					local Clone = Models.Knives[values.skins.knife.model.Scroll]:Clone()  -- blast#6645 / BlastPipe made the scrolling work lol
					return Clone  -- blast#6645 / BlastPipe made the scrolling work lol
				end  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	if key == "Value" then  -- blast#6645 / BlastPipe made the scrolling work lol
		if self.Name == "Auto" and TBLFIND(values.misc.client["gun modifiers"].Jumbobox, "automatic") then  -- blast#6645 / BlastPipe made the scrolling work lol
			return true  -- blast#6645 / BlastPipe made the scrolling work lol
		elseif self.Name == "ReloadTime" and TBLFIND(values.misc.client["gun modifiers"].Jumbobox, "reload") then  -- blast#6645 / BlastPipe made the scrolling work lol
			return 0.001  -- blast#6645 / BlastPipe made the scrolling work lol
		elseif self.Name == "EquipTime" and TBLFIND(values.misc.client["gun modifiers"].Jumbobox, "equip") then  -- blast#6645 / BlastPipe made the scrolling work lol
			return 0.001  -- blast#6645 / BlastPipe made the scrolling work lol
		elseif self.Name == "BuyTime" and TBLFIND(values.misc.client.shop.Jumbobox, "inf time") then  -- blast#6645 / BlastPipe made the scrolling work lol
			return 5  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
	return oldIndex(self, key)  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
