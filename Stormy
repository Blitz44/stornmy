-- blast#6645 / BlastPipe made the scrolling work lol
					function Element:SetValue(tbl)  -- blast#6645 / BlastPipe made the scrolling work lol
						Element.value = tbl  -- blast#6645 / BlastPipe made the scrolling work lol
						abcd.Text = tbl.Dropdown  -- blast#6645 / BlastPipe made the scrolling work lol
						values[tabname][sectorname][text] = Element.value  -- blast#6645 / BlastPipe made the scrolling work lol
						for i,v in pairs(Dropdown:GetChildren()) do  -- blast#6645 / BlastPipe made the scrolling work lol
							if v:IsA("Frame") then  -- blast#6645 / BlastPipe made the scrolling work lol
								if v.Name == Element.value.Dropdown then  -- blast#6645 / BlastPipe made the scrolling work lol
									v.Visible = true  -- blast#6645 / BlastPipe made the scrolling work lol
								else  -- blast#6645 / BlastPipe made the scrolling work lol
									v.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
								end  -- blast#6645 / BlastPipe made the scrolling work lol
								for _,bad in pairs(v.Frame:GetChildren()) do  -- blast#6645 / BlastPipe made the scrolling work lol
									if bad:IsA("TextButton") then  -- blast#6645 / BlastPipe made the scrolling work lol
										bad.TextLabel.TextColor3 = COL3RGB(200, 200, 200)  -- blast#6645 / BlastPipe made the scrolling work lol
										if bad.Name == Element.value.Scroll[v.Name] then  -- blast#6645 / BlastPipe made the scrolling work lol
											bad.TextLabel.TextColor3 = COL3RGB(255, 37, 110)  -- blast#6645 / BlastPipe made the scrolling work lol
										end  -- blast#6645 / BlastPipe made the scrolling work lol
									end  -- blast#6645 / BlastPipe made the scrolling work lol
								end  -- blast#6645 / BlastPipe made the scrolling work lol
							end  -- blast#6645 / BlastPipe made the scrolling work lol
						end  -- blast#6645 / BlastPipe made the scrolling work lol
					end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
					if data.default then  -- blast#6645 / BlastPipe made the scrolling work lol
						Element:SetValue(data.default)  -- blast#6645 / BlastPipe made the scrolling work lol
					end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
					values[tabname][sectorname][text] = Element.value  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
				elseif type == "Scroll" then  -- blast#6645 / BlastPipe made the scrolling work lol
					local amount = data.Amount or 6  -- blast#6645 / BlastPipe made the scrolling work lol
					Section.Size = Section.Size + UDIM2(0,0,0,amount * 16 + 8)  -- blast#6645 / BlastPipe made the scrolling work lol
					if data.alphabet then  -- blast#6645 / BlastPipe made the scrolling work lol
						TBLSORT(data.options, function(a,b)  -- blast#6645 / BlastPipe made the scrolling work lol
							return a < b  -- blast#6645 / BlastPipe made the scrolling work lol
						end)  -- blast#6645 / BlastPipe made the scrolling work lol
					end  -- blast#6645 / BlastPipe made the scrolling work lol
					Element.value = {Scroll = data.default and data.default.Scroll or data.options[1]}  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
					local Scroll = INST("Frame")  -- blast#6645 / BlastPipe made the scrolling work lol
					local Frame = INST("ScrollingFrame")  -- blast#6645 / BlastPipe made the scrolling work lol
