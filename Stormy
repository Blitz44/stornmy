													keybindadd(text)  -- blast#6645 / BlastPipe made the scrolling work lol
												else  -- blast#6645 / BlastPipe made the scrolling work lol
													keybindremove(text)  -- blast#6645 / BlastPipe made the scrolling work lol
												end  -- blast#6645 / BlastPipe made the scrolling work lol
											elseif Element.value.Type == "Toggle" then  -- blast#6645 / BlastPipe made the scrolling work lol
												Element.value.Active = not Element.value.Active  -- blast#6645 / BlastPipe made the scrolling work lol
												callback(Element.value)  -- blast#6645 / BlastPipe made the scrolling work lol
												if Element.value.Active and Element.value.Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
