local function CollisionTBL(obj)  -- blast#6645 / BlastPipe made the scrolling work lol
	if obj:IsA("Accessory") then  -- blast#6645 / BlastPipe made the scrolling work lol
		INSERT(Collision, obj)  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	if obj:IsA("Part") then  -- blast#6645 / BlastPipe made the scrolling work lol
		if obj.Name == "HeadHB" or obj.Name == "FakeHead" then  -- blast#6645 / BlastPipe made the scrolling work lol
			INSERT(Collision, obj)  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
