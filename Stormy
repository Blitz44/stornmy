world:Element("Dropdown", "skybox", {options = {"none", "nebula", "vaporwave", "clouds"}}, function(tbl)  -- blast#6645 / BlastPipe made the scrolling work lol
	local sky = tbl.Dropdown  -- blast#6645 / BlastPipe made the scrolling work lol
	if sky ~= "none" then  -- blast#6645 / BlastPipe made the scrolling work lol
		if Lighting:FindFirstChildOfClass("Sky") then Lighting:FindFirstChildOfClass("Sky"):Destroy() end  -- blast#6645 / BlastPipe made the scrolling work lol
		local skybox = INST("Sky")  -- blast#6645 / BlastPipe made the scrolling work lol
		skybox.SkyboxLf = Skyboxes[sky].SkyboxLf  -- blast#6645 / BlastPipe made the scrolling work lol
		skybox.SkyboxBk = Skyboxes[sky].SkyboxBk  -- blast#6645 / BlastPipe made the scrolling work lol
		skybox.SkyboxDn = Skyboxes[sky].SkyboxDn  -- blast#6645 / BlastPipe made the scrolling work lol
		skybox.SkyboxFt = Skyboxes[sky].SkyboxFt  -- blast#6645 / BlastPipe made the scrolling work lol
		skybox.SkyboxRt = Skyboxes[sky].SkyboxRt  -- blast#6645 / BlastPipe made the scrolling work lol
		skybox.SkyboxUp = Skyboxes[sky].SkyboxUp  -- blast#6645 / BlastPipe made the scrolling work lol
		skybox.Name = "override"  -- blast#6645 / BlastPipe made the scrolling work lol
		skybox.Parent = Lighting  -- blast#6645 / BlastPipe made the scrolling work lol
	else  -- blast#6645 / BlastPipe made the scrolling work lol
		if Lighting:FindFirstChildOfClass("Sky") then Lighting:FindFirstChildOfClass("Sky"):Destroy() end  -- blast#6645 / BlastPipe made the scrolling work lol
		if oldSkybox ~= nil then oldSkybox:Clone().Parent = Lighting end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
end)  -- blast#6645 / BlastPipe made the scrolling work lol
