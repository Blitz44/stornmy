							return a < b  -- blast#6645 / BlastPipe made the scrolling work lol
						end)  -- blast#6645 / BlastPipe made the scrolling work lol
						joe = copy  -- blast#6645 / BlastPipe made the scrolling work lol
					else  -- blast#6645 / BlastPipe made the scrolling work lol
						for i,v in pairs(data.options) do  -- blast#6645 / BlastPipe made the scrolling work lol
							INSERT(joe, i)  -- blast#6645 / BlastPipe made the scrolling work lol
						end  -- blast#6645 / BlastPipe made the scrolling work lol
					end  -- blast#6645 / BlastPipe made the scrolling work lol
