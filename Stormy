													break  -- blast#6645 / BlastPipe made the scrolling work lol
												end  -- blast#6645 / BlastPipe made the scrolling work lol
											else  -- blast#6645 / BlastPipe made the scrolling work lol
												local penetration = Client.gun.Penetration.Value * 0.01  -- blast#6645 / BlastPipe made the scrolling work lol
												local limit = 0  -- blast#6645 / BlastPipe made the scrolling work lol
												local dmgmodifier = 1  -- blast#6645 / BlastPipe made the scrolling work lol
												for i = 1, #Hits do  -- blast#6645 / BlastPipe made the scrolling work lol
													local data = Hits[i]  -- blast#6645 / BlastPipe made the scrolling work lol
													local part = data["Hit"]  -- blast#6645 / BlastPipe made the scrolling work lol
													local pos = data["Position"]  -- blast#6645 / BlastPipe made the scrolling work lol
													local modifier = 1  -- blast#6645 / BlastPipe made the scrolling work lol
													if part.Material == Enum.Material.DiamondPlate then  -- blast#6645 / BlastPipe made the scrolling work lol
														modifier = 3  -- blast#6645 / BlastPipe made the scrolling work lol
													end  -- blast#6645 / BlastPipe made the scrolling work lol
													if part.Material == Enum.Material.CorrodedMetal or part.Material == Enum.Material.Metal or part.Material == Enum.Material.Concrete or part.Material == Enum.Material.Brick then  -- blast#6645 / BlastPipe made the scrolling work lol
														modifier = 2  -- blast#6645 / BlastPipe made the scrolling work lol
													end  -- blast#6645 / BlastPipe made the scrolling work lol
													if part.Name == "Grate" or part.Material == Enum.Material.Wood or part.Material == Enum.Material.WoodPlanks then  -- blast#6645 / BlastPipe made the scrolling work lol
														modifier = 0.1  -- blast#6645 / BlastPipe made the scrolling work lol
													end  -- blast#6645 / BlastPipe made the scrolling work lol
													if part.Name == "nowallbang" then  -- blast#6645 / BlastPipe made the scrolling work lol
														modifier = 100  -- blast#6645 / BlastPipe made the scrolling work lol
													end  -- blast#6645 / BlastPipe made the scrolling work lol
													if part:FindFirstChild("PartModifier") then  -- blast#6645 / BlastPipe made the scrolling work lol
														modifier = part.PartModifier.Value  -- blast#6645 / BlastPipe made the scrolling work lol
													end  -- blast#6645 / BlastPipe made the scrolling work lol
													if part.Transparency == 1 or part.CanCollide == false or part.Name == "Glass" or part.Name == "Cardboard" then  -- blast#6645 / BlastPipe made the scrolling work lol
														modifier = 0  -- blast#6645 / BlastPipe made the scrolling work lol
													end  -- blast#6645 / BlastPipe made the scrolling work lol
													local direction = (Hitbox.Position - pos).unit * CLAMP(Client.gun.Range.Value, 1, 100)  -- blast#6645 / BlastPipe made the scrolling work lol
													local ray = RAY(pos + direction * 1, direction * -2)  -- blast#6645 / BlastPipe made the scrolling work lol
													local _,endpos = workspace:FindPartOnRayWithWhitelist(ray, {part}, true)  -- blast#6645 / BlastPipe made the scrolling work lol
													local thickness = (endpos - pos).Magnitude  -- blast#6645 / BlastPipe made the scrolling work lol
													thickness = thickness * modifier  -- blast#6645 / BlastPipe made the scrolling work lol
													limit = MIN(penetration, limit + thickness)  -- blast#6645 / BlastPipe made the scrolling work lol
													dmgmodifier = 1 - limit / penetration  -- blast#6645 / BlastPipe made the scrolling work lol
												end  -- blast#6645 / BlastPipe made the scrolling work lol
												local Damage = Client.gun.DMG.Value * Multipliers[EndHit.Name] * dmgmodifier  -- blast#6645 / BlastPipe made the scrolling work lol
