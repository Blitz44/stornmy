	local first = true  -- blast#6645 / BlastPipe made the scrolling work lol
	local currenttab  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
	function menu:Tab(text)  -- blast#6645 / BlastPipe made the scrolling work lol
		local tabname  -- blast#6645 / BlastPipe made the scrolling work lol
		tabname = text  -- blast#6645 / BlastPipe made the scrolling work lol
		local Tab = {}  -- blast#6645 / BlastPipe made the scrolling work lol
		values[tabname] = {}  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
		local TextButton = INST("TextButton")  -- blast#6645 / BlastPipe made the scrolling work lol
