local LagTick = 0  -- blast#6645 / BlastPipe made the scrolling work lol
local fakelag = rage:Sector("fakelag", "Right")  -- blast#6645 / BlastPipe made the scrolling work lol
fakelag:Element("Toggle", "enabled", {default = {Toggle = false}}, function(tbl)  -- blast#6645 / BlastPipe made the scrolling work lol
	if tbl.Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
	else  -- blast#6645 / BlastPipe made the scrolling work lol
		FakelagFolder:ClearAllChildren()  -- blast#6645 / BlastPipe made the scrolling work lol
		game:GetService("NetworkClient"):SetOutgoingKBPSLimit(9e9)  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
end)  -- blast#6645 / BlastPipe made the scrolling work lol
fakelag:Element("Dropdown", "amount", {options = {"static", "dynamic"}})  -- blast#6645 / BlastPipe made the scrolling work lol
