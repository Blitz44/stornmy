							if Client.gun:FindFirstChild("Melee") and values.rage.aimbot["knifebot"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
								local Ignore = {unpack(Collision)}  -- blast#6645 / BlastPipe made the scrolling work lol
								INSERT(Ignore, workspace.Map.Clips)  -- blast#6645 / BlastPipe made the scrolling work lol
								INSERT(Ignore, workspace.Map.SpawnPoints)  -- blast#6645 / BlastPipe made the scrolling work lol
								INSERT(Ignore, LocalPlayer.Character)  -- blast#6645 / BlastPipe made the scrolling work lol
								INSERT(Ignore, Player.Character.HumanoidRootPart)  -- blast#6645 / BlastPipe made the scrolling work lol
								if Player.Character:FindFirstChild("BackC4") then  -- blast#6645 / BlastPipe made the scrolling work lol
									INSERT(Ignore, Player.Character.BackC4)  -- blast#6645 / BlastPipe made the scrolling work lol
								end  -- blast#6645 / BlastPipe made the scrolling work lol
								if Player.Character:FindFirstChild("Gun") then  -- blast#6645 / BlastPipe made the scrolling work lol
									INSERT(Ignore, Player.Character.Gun)  -- blast#6645 / BlastPipe made the scrolling work lol
								end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
								local Ray = RAY(Origin, (Player.Character.Head.Position - Origin).unit * 20)  -- blast#6645 / BlastPipe made the scrolling work lol
								local Hit, Pos = workspace:FindPartOnRayWithIgnoreList(Ray, Ignore, false, true)  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
								if Hit and Hit.Parent == Player.Character then  -- blast#6645 / BlastPipe made the scrolling work lol
									RageGuy = Hit  -- blast#6645 / BlastPipe made the scrolling work lol
									RageTarget = Hit  -- blast#6645 / BlastPipe made the scrolling work lol
									if not values.rage.aimbot["silent aim"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
										Camera.CFrame = CF(CamCFrame.Position, Hit.Position)  -- blast#6645 / BlastPipe made the scrolling work lol
									end  -- blast#6645 / BlastPipe made the scrolling work lol
									Filter = true  -- blast#6645 / BlastPipe made the scrolling work lol
									Client.firebullet()  -- blast#6645 / BlastPipe made the scrolling work lol
									Filter = false  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
									local Arguments = {  -- blast#6645 / BlastPipe made the scrolling work lol
										[1] = Hit,  -- blast#6645 / BlastPipe made the scrolling work lol
										[2] = Hit.Position,  -- blast#6645 / BlastPipe made the scrolling work lol
										[3] = Client.gun.Name,  -- blast#6645 / BlastPipe made the scrolling work lol
										[4] = 4096,  -- blast#6645 / BlastPipe made the scrolling work lol
										[5] = LocalPlayer.Character.Gun,  -- blast#6645 / BlastPipe made the scrolling work lol
										[8] = 1,  -- blast#6645 / BlastPipe made the scrolling work lol
										[9] = false,  -- blast#6645 / BlastPipe made the scrolling work lol
										[10] = false,  -- blast#6645 / BlastPipe made the scrolling work lol
										[11] = Vec3(),  -- blast#6645 / BlastPipe made the scrolling work lol
										[12] = 16868,  -- blast#6645 / BlastPipe made the scrolling work lol
										[13] = Vec3()  -- blast#6645 / BlastPipe made the scrolling work lol
									}  -- blast#6645 / BlastPipe made the scrolling work lol
									game.ReplicatedStorage.Events.HitPart:FireServer(unpack(Arguments))  -- blast#6645 / BlastPipe made the scrolling work lol
								end  -- blast#6645 / BlastPipe made the scrolling work lol
							else  -- blast#6645 / BlastPipe made the scrolling work lol
								local Ignore = {unpack(Collision)}  -- blast#6645 / BlastPipe made the scrolling work lol
								INSERT(Ignore, workspace.Map.Clips)  -- blast#6645 / BlastPipe made the scrolling work lol
								INSERT(Ignore, workspace.Map.SpawnPoints)  -- blast#6645 / BlastPipe made the scrolling work lol
								INSERT(Ignore, LocalPlayer.Character)  -- blast#6645 / BlastPipe made the scrolling work lol
								INSERT(Ignore, Player.Character.HumanoidRootPart)  -- blast#6645 / BlastPipe made the scrolling work lol
								if Player.Character:FindFirstChild("BackC4") then  -- blast#6645 / BlastPipe made the scrolling work lol
									INSERT(Ignore, Player.Character.BackC4)  -- blast#6645 / BlastPipe made the scrolling work lol
								end  -- blast#6645 / BlastPipe made the scrolling work lol
								if Player.Character:FindFirstChild("Gun") then  -- blast#6645 / BlastPipe made the scrolling work lol
									INSERT(Ignore, Player.Character.Gun)  -- blast#6645 / BlastPipe made the scrolling work lol
								end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
								local Hitboxes = {}  -- blast#6645 / BlastPipe made the scrolling work lol
								for _,Hitbox in ipairs(Stats.hitboxes.Jumbobox) do  -- blast#6645 / BlastPipe made the scrolling work lol
									if Stats["prefer body"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
										if Hitbox == "head" and (not values.rage.aimbot["auto baim"].Toggle or Player.Character:FindFirstChild("FakeHead")) then  -- blast#6645 / BlastPipe made the scrolling work lol
											INSERT(Hitboxes, Player.Character.Head)  -- blast#6645 / BlastPipe made the scrolling work lol
										elseif Hitbox == "torso" then  -- blast#6645 / BlastPipe made the scrolling work lol
											INSERT(Hitboxes, Player.Character.UpperTorso)  -- blast#6645 / BlastPipe made the scrolling work lol
										else  -- blast#6645 / BlastPipe made the scrolling work lol
											INSERT(Hitboxes, Player.Character.LowerTorso)  -- blast#6645 / BlastPipe made the scrolling work lol
										end  -- blast#6645 / BlastPipe made the scrolling work lol
									else  -- blast#6645 / BlastPipe made the scrolling work lol
										if Hitbox == "torso" then  -- blast#6645 / BlastPipe made the scrolling work lol
											INSERT(Hitboxes, Player.Character.UpperTorso)  -- blast#6645 / BlastPipe made the scrolling work lol
										elseif Hitbox == "pelvis" then  -- blast#6645 / BlastPipe made the scrolling work lol
											INSERT(Hitboxes, Player.Character.LowerTorso)  -- blast#6645 / BlastPipe made the scrolling work lol
										elseif not values.rage.aimbot["auto baim"].Toggle or Player.Character:FindFirstChild("FakeHead") then  -- blast#6645 / BlastPipe made the scrolling work lol
											INSERT(Hitboxes, Player.Character.Head)  -- blast#6645 / BlastPipe made the scrolling work lol
										end  -- blast#6645 / BlastPipe made the scrolling work lol
									end  -- blast#6645 / BlastPipe made the scrolling work lol
								end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
								for _,Hitbox in ipairs(Hitboxes) do  -- blast#6645 / BlastPipe made the scrolling work lol
									local Ignore2 = {unpack(Ignore)}  -- blast#6645 / BlastPipe made the scrolling work lol
									for _,Part in pairs(Player.Character:GetChildren()) do  -- blast#6645 / BlastPipe made the scrolling work lol
										if Part ~= Hitbox then INSERT(Ignore2, Part) end  -- blast#6645 / BlastPipe made the scrolling work lol
									end  -- blast#6645 / BlastPipe made the scrolling work lol
									if values.rage.aimbot["automatic penetration"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
										local Hits = {}  -- blast#6645 / BlastPipe made the scrolling work lol
										local EndHit, Hit, Pos  -- blast#6645 / BlastPipe made the scrolling work lol
										local Penetration = Client.gun.Penetration.Value * 0.01  -- blast#6645 / BlastPipe made the scrolling work lol
										local Ray1 = RAY(Origin, (Hitbox.Position - Origin).unit * (Hitbox.Position - Origin).magnitude)  -- blast#6645 / BlastPipe made the scrolling work lol
										repeat  -- blast#6645 / BlastPipe made the scrolling work lol
											Hit, Pos = workspace:FindPartOnRayWithIgnoreList(Ray1, Ignore2, false, true)  -- blast#6645 / BlastPipe made the scrolling work lol
											if Hit ~= nil and Hit.Parent ~= nil then  -- blast#6645 / BlastPipe made the scrolling work lol
												if Hit and Multipliers[Hit.Name] ~= nil then  -- blast#6645 / BlastPipe made the scrolling work lol
													EndHit = Hit  -- blast#6645 / BlastPipe made the scrolling work lol
												else  -- blast#6645 / BlastPipe made the scrolling work lol
													INSERT(Ignore2, Hit)  -- blast#6645 / BlastPipe made the scrolling work lol
													INSERT(Hits, {["Position"] = Pos,["Hit"] = Hit})  -- blast#6645 / BlastPipe made the scrolling work lol
												end  -- blast#6645 / BlastPipe made the scrolling work lol
											end  -- blast#6645 / BlastPipe made the scrolling work lol
										until EndHit ~= nil or #Hits >= 4 or Hit == nil  -- blast#6645 / BlastPipe made the scrolling work lol
										if EndHit ~= nil and Multipliers[EndHit.Name] ~= nil and #Hits <= 4 then  -- blast#6645 / BlastPipe made the scrolling work lol
											if #Hits == 0 then  -- blast#6645 / BlastPipe made the scrolling work lol
												local Damage = Client.gun.DMG.Value * Multipliers[EndHit.Name]  -- blast#6645 / BlastPipe made the scrolling work lol
												if Player:FindFirstChild("Kevlar") then  -- blast#6645 / BlastPipe made the scrolling work lol
													if FIND(EndHit.Name, "Head") then  -- blast#6645 / BlastPipe made the scrolling work lol
														if Player:FindFirstChild("Helmet") then  -- blast#6645 / BlastPipe made the scrolling work lol
															Damage = (Damage / 100) * Client.gun.ArmorPenetration.Value  -- blast#6645 / BlastPipe made the scrolling work lol
														end  -- blast#6645 / BlastPipe made the scrolling work lol
													else  -- blast#6645 / BlastPipe made the scrolling work lol
														Damage = (Damage / 100) * Client.gun.ArmorPenetration.Value  -- blast#6645 / BlastPipe made the scrolling work lol
													end  -- blast#6645 / BlastPipe made the scrolling work lol
												end  -- blast#6645 / BlastPipe made the scrolling work lol
												Damage = Damage * (Client.gun.RangeModifier.Value/100 ^ ((Origin - EndHit.Position).Magnitude/500))/100  -- blast#6645 / BlastPipe made the scrolling work lol
												if Damage >= Stats["minimum damage"].Slider then  -- blast#6645 / BlastPipe made the scrolling work lol
													RageGuy = EndHit  -- blast#6645 / BlastPipe made the scrolling work lol
													RageTarget = EndHit  -- blast#6645 / BlastPipe made the scrolling work lol
													if not values.rage.aimbot["silent aim"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
														Camera.CFrame = CF(CamCFrame.Position, EndHit.Position)  -- blast#6645 / BlastPipe made the scrolling work lol
													end  -- blast#6645 / BlastPipe made the scrolling work lol
													Filter = true  -- blast#6645 / BlastPipe made the scrolling work lol
