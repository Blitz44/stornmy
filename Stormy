						for _,Player in pairs(Players:GetPlayers()) do  -- blast#6645 / BlastPipe made the scrolling work lol
							if Player.Character and Player.Character:FindFirstChild("Humanoid") and Player.Character:FindFirstChild("Humanoid").Health > 0 then  -- blast#6645 / BlastPipe made the scrolling work lol
