-- blast#6645 / BlastPipe made the scrolling work lol
local objects = {}  -- blast#6645 / BlastPipe made the scrolling work lol
local utility = {}  -- blast#6645 / BlastPipe made the scrolling work lol
do  -- blast#6645 / BlastPipe made the scrolling work lol
	utility.default = {  -- blast#6645 / BlastPipe made the scrolling work lol
		Line = {  -- blast#6645 / BlastPipe made the scrolling work lol
			Thickness = 1.5,  -- blast#6645 / BlastPipe made the scrolling work lol
			Color = COL3RGB(255, 255, 255),  -- blast#6645 / BlastPipe made the scrolling work lol
			Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
		},  -- blast#6645 / BlastPipe made the scrolling work lol
		Text = {  -- blast#6645 / BlastPipe made the scrolling work lol
			Size = 13,  -- blast#6645 / BlastPipe made the scrolling work lol
			Center = true,  -- blast#6645 / BlastPipe made the scrolling work lol
			Outline = true,  -- blast#6645 / BlastPipe made the scrolling work lol
			Font = Drawing.Fonts.Plex,  -- blast#6645 / BlastPipe made the scrolling work lol
			Color = COL3RGB(255, 255, 255),  -- blast#6645 / BlastPipe made the scrolling work lol
			Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
		},  -- blast#6645 / BlastPipe made the scrolling work lol
		Square = {  -- blast#6645 / BlastPipe made the scrolling work lol
			Thickness = 1.5,  -- blast#6645 / BlastPipe made the scrolling work lol
			Filled = false,  -- blast#6645 / BlastPipe made the scrolling work lol
			Color = COL3RGB(255, 255, 255),  -- blast#6645 / BlastPipe made the scrolling work lol
			Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
		},  -- blast#6645 / BlastPipe made the scrolling work lol
	}  -- blast#6645 / BlastPipe made the scrolling work lol
	function utility.create(type, isOutline)  -- blast#6645 / BlastPipe made the scrolling work lol
		local drawing = Drawing.new(type)  -- blast#6645 / BlastPipe made the scrolling work lol
		for i, v in pairs(utility.default[type]) do  -- blast#6645 / BlastPipe made the scrolling work lol
			drawing[i] = v  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
		if isOutline then  -- blast#6645 / BlastPipe made the scrolling work lol
			drawing.Color = COL3(0,0,0)  -- blast#6645 / BlastPipe made the scrolling work lol
			drawing.Thickness = 3  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
		return drawing  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	function utility.add(plr)  -- blast#6645 / BlastPipe made the scrolling work lol
		if not objects[plr] then  -- blast#6645 / BlastPipe made the scrolling work lol
			objects[plr] = {  -- blast#6645 / BlastPipe made the scrolling work lol
				Name = utility.create("Text"),  -- blast#6645 / BlastPipe made the scrolling work lol
				Weapon = utility.create("Text"),  -- blast#6645 / BlastPipe made the scrolling work lol
				Armor = utility.create("Text"),  -- blast#6645 / BlastPipe made the scrolling work lol
				BoxOutline = utility.create("Square", true),  -- blast#6645 / BlastPipe made the scrolling work lol
				Box = utility.create("Square"),  -- blast#6645 / BlastPipe made the scrolling work lol
				HealthOutline = utility.create("Line", true),  -- blast#6645 / BlastPipe made the scrolling work lol
				Health = utility.create("Line"),  -- blast#6645 / BlastPipe made the scrolling work lol
			}  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	for _,plr in pairs(Players:GetPlayers()) do  -- blast#6645 / BlastPipe made the scrolling work lol
		if Player ~= LocalPlayer then  -- blast#6645 / BlastPipe made the scrolling work lol
			utility.add(plr)  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	Players.PlayerAdded:Connect(utility.add)  -- blast#6645 / BlastPipe made the scrolling work lol
	Players.PlayerRemoving:Connect(function(plr)  -- blast#6645 / BlastPipe made the scrolling work lol
		wait()  -- blast#6645 / BlastPipe made the scrolling work lol
		if objects[plr] then  -- blast#6645 / BlastPipe made the scrolling work lol
			for i,v in pairs(objects[plr]) do  -- blast#6645 / BlastPipe made the scrolling work lol
				for i2,v2 in pairs(v) do  -- blast#6645 / BlastPipe made the scrolling work lol
					if v then  -- blast#6645 / BlastPipe made the scrolling work lol
						v:Remove()  -- blast#6645 / BlastPipe made the scrolling work lol
					end  -- blast#6645 / BlastPipe made the scrolling work lol
				end  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
			objects[plr] = nil  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end)  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
