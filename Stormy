-- blast#6645 / BlastPipe made the scrolling work lol
					function Element:SetValue(value)  -- blast#6645 / BlastPipe made the scrolling work lol
						Element.value = value  -- blast#6645 / BlastPipe made the scrolling work lol
						local a  -- blast#6645 / BlastPipe made the scrolling work lol
						if min > 0 then  -- blast#6645 / BlastPipe made the scrolling work lol
							a = ((Element.value.Slider - min)) / (max-min)  -- blast#6645 / BlastPipe made the scrolling work lol
						else  -- blast#6645 / BlastPipe made the scrolling work lol
							a = (Element.value.Slider-min)/(max-min)  -- blast#6645 / BlastPipe made the scrolling work lol
						end  -- blast#6645 / BlastPipe made the scrolling work lol
						Value.Text = Element.value.Slider  -- blast#6645 / BlastPipe made the scrolling work lol
						Frame.Size = UDIM2(a,0,1,0)  -- blast#6645 / BlastPipe made the scrolling work lol
						values[tabname][sectorname][text] = Element.value  -- blast#6645 / BlastPipe made the scrolling work lol
						callback(value)  -- blast#6645 / BlastPipe made the scrolling work lol
					end  -- blast#6645 / BlastPipe made the scrolling work lol
					local a  -- blast#6645 / BlastPipe made the scrolling work lol
					if min > 0 then  -- blast#6645 / BlastPipe made the scrolling work lol
						a = ((Element.value.Slider - min)) / (max-min)  -- blast#6645 / BlastPipe made the scrolling work lol
					else  -- blast#6645 / BlastPipe made the scrolling work lol
						a = (Element.value.Slider-min)/(max-min)  -- blast#6645 / BlastPipe made the scrolling work lol
					end  -- blast#6645 / BlastPipe made the scrolling work lol
					Value.Text = Element.value.Slider  -- blast#6645 / BlastPipe made the scrolling work lol
					Frame.Size = UDIM2(a,0,1,0)  -- blast#6645 / BlastPipe made the scrolling work lol
					values[tabname][sectorname][text] = Element.value  -- blast#6645 / BlastPipe made the scrolling work lol
					local uis = game:GetService("UserInputService")  -- blast#6645 / BlastPipe made the scrolling work lol
					local mouse = game.Players.LocalPlayer:GetMouse()  -- blast#6645 / BlastPipe made the scrolling work lol
					local val  -- blast#6645 / BlastPipe made the scrolling work lol
					Button.MouseButton1Down:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
						Frame.Size = UDIM2(0, CLAMP(mouse.X - Frame.AbsolutePosition.X, 0, 175), 0, 5)  -- blast#6645 / BlastPipe made the scrolling work lol
						val = FLOOR((((tonumber(max) - tonumber(min)) / 175) * Frame.AbsoluteSize.X) + tonumber(min)) or 0  -- blast#6645 / BlastPipe made the scrolling work lol
						Value.Text = val  -- blast#6645 / BlastPipe made the scrolling work lol
						Element.value.Slider = val  -- blast#6645 / BlastPipe made the scrolling work lol
						values[tabname][sectorname][text] = Element.value  -- blast#6645 / BlastPipe made the scrolling work lol
						callback(Element.value)  -- blast#6645 / BlastPipe made the scrolling work lol
						moveconnection = mouse.Move:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
							Frame.Size = UDIM2(0, CLAMP(mouse.X - Frame.AbsolutePosition.X, 0, 175), 0, 5)  -- blast#6645 / BlastPipe made the scrolling work lol
							val = FLOOR((((tonumber(max) - tonumber(min)) / 175) * Frame.AbsoluteSize.X) + tonumber(min))  -- blast#6645 / BlastPipe made the scrolling work lol
							Value.Text = val  -- blast#6645 / BlastPipe made the scrolling work lol
							Element.value.Slider = val  -- blast#6645 / BlastPipe made the scrolling work lol
							values[tabname][sectorname][text] = Element.value  -- blast#6645 / BlastPipe made the scrolling work lol
							callback(Element.value)  -- blast#6645 / BlastPipe made the scrolling work lol
						end)  -- blast#6645 / BlastPipe made the scrolling work lol
						releaseconnection = uis.InputEnded:Connect(function(Mouse)  -- blast#6645 / BlastPipe made the scrolling work lol
							if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then  -- blast#6645 / BlastPipe made the scrolling work lol
								Frame.Size = UDIM2(0, CLAMP(mouse.X - Frame.AbsolutePosition.X, 0, 175), 0, 5)  -- blast#6645 / BlastPipe made the scrolling work lol
								val = FLOOR((((tonumber(max) - tonumber(min)) / 175) * Frame.AbsoluteSize.X) + tonumber(min))  -- blast#6645 / BlastPipe made the scrolling work lol
								values[tabname][sectorname][text] = Element.value  -- blast#6645 / BlastPipe made the scrolling work lol
								callback(Element.value)  -- blast#6645 / BlastPipe made the scrolling work lol
								moveconnection:Disconnect()  -- blast#6645 / BlastPipe made the scrolling work lol
								releaseconnection:Disconnect()  -- blast#6645 / BlastPipe made the scrolling work lol
							end  -- blast#6645 / BlastPipe made the scrolling work lol
						end)  -- blast#6645 / BlastPipe made the scrolling work lol
					end)  -- blast#6645 / BlastPipe made the scrolling work lol
				elseif type == "Button" then  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
					Section.Size = Section.Size + UDIM2(0,0,0,24)  -- blast#6645 / BlastPipe made the scrolling work lol
					local Button = INST("Frame")  -- blast#6645 / BlastPipe made the scrolling work lol
					local Button_2 = INST("TextButton")  -- blast#6645 / BlastPipe made the scrolling work lol
					local TextLabel = INST("TextLabel")  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
					Button.Name = "Button"  -- blast#6645 / BlastPipe made the scrolling work lol
					Button.Parent = Inner  -- blast#6645 / BlastPipe made the scrolling work lol
					Button.BackgroundColor3 = COL3RGB(255, 255, 255)  -- blast#6645 / BlastPipe made the scrolling work lol
					Button.BackgroundTransparency = 1.000  -- blast#6645 / BlastPipe made the scrolling work lol
					Button.Position = UDIM2(0, 0, 0.236059487, 0)  -- blast#6645 / BlastPipe made the scrolling work lol
					Button.Size = UDIM2(1, 0, 0, 24)  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
					Button_2.Name = "Button"  -- blast#6645 / BlastPipe made the scrolling work lol
					Button_2.Parent = Button  -- blast#6645 / BlastPipe made the scrolling work lol
					Button_2.BackgroundColor3 = COL3RGB(33, 35, 47)  -- blast#6645 / BlastPipe made the scrolling work lol
					Button_2.BorderColor3 = COL3RGB(27, 27, 35)  -- blast#6645 / BlastPipe made the scrolling work lol
					Button_2.Position = UDIM2(0, 30, 0.5, -9)  -- blast#6645 / BlastPipe made the scrolling work lol
					Button_2.Size = UDIM2(0, 175, 0, 18)  -- blast#6645 / BlastPipe made the scrolling work lol
					Button_2.AutoButtonColor = false  -- blast#6645 / BlastPipe made the scrolling work lol
					Button_2.Font = Enum.Font.SourceSans  -- blast#6645 / BlastPipe made the scrolling work lol
					Button_2.Text = ""  -- blast#6645 / BlastPipe made the scrolling work lol
					Button_2.TextColor3 = COL3RGB(0, 0, 0)  -- blast#6645 / BlastPipe made the scrolling work lol
					Button_2.TextSize = 14.000  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
					TextLabel.Parent = Button_2  -- blast#6645 / BlastPipe made the scrolling work lol
					TextLabel.BackgroundColor3 = COL3RGB(255, 255, 255)  -- blast#6645 / BlastPipe made the scrolling work lol
					TextLabel.BackgroundTransparency = 1.000  -- blast#6645 / BlastPipe made the scrolling work lol
					TextLabel.BorderColor3 = COL3RGB(27, 42, 53)  -- blast#6645 / BlastPipe made the scrolling work lol
					TextLabel.Size = UDIM2(1, 0, 1, 0)  -- blast#6645 / BlastPipe made the scrolling work lol
					TextLabel.Font = Enum.Font.SourceSansSemibold  -- blast#6645 / BlastPipe made the scrolling work lol
					TextLabel.Text = text  -- blast#6645 / BlastPipe made the scrolling work lol
					TextLabel.TextColor3 = COL3RGB(200, 200, 200)  -- blast#6645 / BlastPipe made the scrolling work lol
					TextLabel.TextSize = 14.000  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
					function Element:SetValue()  -- blast#6645 / BlastPipe made the scrolling work lol
					end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
					Button_2.MouseButton1Down:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
						TextLabel.TextColor3 = COL3RGB(175, 42, 86)  -- blast#6645 / BlastPipe made the scrolling work lol
						library:Tween(TextLabel, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextColor3 = COL3RGB(200, 200, 200)})  -- blast#6645 / BlastPipe made the scrolling work lol
						callback()  -- blast#6645 / BlastPipe made the scrolling work lol
					end)  -- blast#6645 / BlastPipe made the scrolling work lol
					Button_2.MouseEnter:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
						library:Tween(TextLabel, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextColor3 = COL3RGB(255, 255, 255)})  -- blast#6645 / BlastPipe made the scrolling work lol
					end)  -- blast#6645 / BlastPipe made the scrolling work lol
					Button_2.MouseLeave:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
						library:Tween(TextLabel, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextColor3 = COL3RGB(200, 200, 200)})  -- blast#6645 / BlastPipe made the scrolling work lol
					end)  -- blast#6645 / BlastPipe made the scrolling work lol
				end  -- blast#6645 / BlastPipe made the scrolling work lol
				ConfigLoad:Connect(function(cfg)  -- blast#6645 / BlastPipe made the scrolling work lol
					pcall(function()  -- blast#6645 / BlastPipe made the scrolling work lol
						local fix = library:ConfigFix(cfg)  -- blast#6645 / BlastPipe made the scrolling work lol
						if fix[tabname][sectorname][text] ~= nil then  -- blast#6645 / BlastPipe made the scrolling work lol
							Element:SetValue(fix[tabname][sectorname][text])  -- blast#6645 / BlastPipe made the scrolling work lol
						end  -- blast#6645 / BlastPipe made the scrolling work lol
					end)  -- blast#6645 / BlastPipe made the scrolling work lol
				end)  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
				return Element  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
			return Sector  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
		return Tab  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
	Ova.Parent = game.CoreGui  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
	return menu  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
