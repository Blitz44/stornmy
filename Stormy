							end  -- blast#6645 / BlastPipe made the scrolling work lol
							Keybind.MouseButton1Down:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
								if not binding then  -- blast#6645 / BlastPipe made the scrolling work lol
									wait()  -- blast#6645 / BlastPipe made the scrolling work lol
									binding = true  -- blast#6645 / BlastPipe made the scrolling work lol
									Keybind.Text = "..."  -- blast#6645 / BlastPipe made the scrolling work lol
