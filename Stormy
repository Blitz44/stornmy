			for _,obj in pairs(Character:GetChildren()) do  -- blast#6645 / BlastPipe made the scrolling work lol
				if obj:IsA("BasePart") and Player ~= LocalPlayer and obj.Name ~= "HumanoidRootPart" and obj.Name ~= "Head" and obj.Name ~= "BackC4" and obj.Name ~= "HeadHB" then  -- blast#6645 / BlastPipe made the scrolling work lol
					local VisibleCham = INST("BoxHandleAdornment")  -- blast#6645 / BlastPipe made the scrolling work lol
