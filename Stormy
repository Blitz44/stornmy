-- blast#6645 / BlastPipe made the scrolling work lol
					local function updatetext()  -- blast#6645 / BlastPipe made the scrolling work lol
						local old = {}  -- blast#6645 / BlastPipe made the scrolling work lol
						for i,v in ipairs(data.options) do  -- blast#6645 / BlastPipe made the scrolling work lol
							if TBLFIND(Element.value.Jumbobox, v) then  -- blast#6645 / BlastPipe made the scrolling work lol
								INSERT(old, v)  -- blast#6645 / BlastPipe made the scrolling work lol
							else  -- blast#6645 / BlastPipe made the scrolling work lol
							end  -- blast#6645 / BlastPipe made the scrolling work lol
						end  -- blast#6645 / BlastPipe made the scrolling work lol
						local str = ""  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
						if #old == 0 then  -- blast#6645 / BlastPipe made the scrolling work lol
							str = "..."  -- blast#6645 / BlastPipe made the scrolling work lol
						else  -- blast#6645 / BlastPipe made the scrolling work lol
							if #old == 1 then  -- blast#6645 / BlastPipe made the scrolling work lol
								str = old[1]  -- blast#6645 / BlastPipe made the scrolling work lol
							else  -- blast#6645 / BlastPipe made the scrolling work lol
								for i,v in ipairs(old) do  -- blast#6645 / BlastPipe made the scrolling work lol
									if i == 1 then  -- blast#6645 / BlastPipe made the scrolling work lol
										str = v  -- blast#6645 / BlastPipe made the scrolling work lol
									else  -- blast#6645 / BlastPipe made the scrolling work lol
										if i > 2 then  -- blast#6645 / BlastPipe made the scrolling work lol
											if i < 4 then  -- blast#6645 / BlastPipe made the scrolling work lol
												str = str..",  ..."  -- blast#6645 / BlastPipe made the scrolling work lol
											end  -- blast#6645 / BlastPipe made the scrolling work lol
										else  -- blast#6645 / BlastPipe made the scrolling work lol
											str = str..",  "..v  -- blast#6645 / BlastPipe made the scrolling work lol
										end  -- blast#6645 / BlastPipe made the scrolling work lol
									end  -- blast#6645 / BlastPipe made the scrolling work lol
								end  -- blast#6645 / BlastPipe made the scrolling work lol
							end  -- blast#6645 / BlastPipe made the scrolling work lol
						end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
						abcd.Text = str  -- blast#6645 / BlastPipe made the scrolling work lol
					end  -- blast#6645 / BlastPipe made the scrolling work lol
					for i,v in ipairs(data.options) do  -- blast#6645 / BlastPipe made the scrolling work lol
						do  -- blast#6645 / BlastPipe made the scrolling work lol
							local Button = INST("TextButton")  -- blast#6645 / BlastPipe made the scrolling work lol
							local TextLabel = INST("TextLabel")  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
							Button.Name = v  -- blast#6645 / BlastPipe made the scrolling work lol
							Button.Parent = Drop  -- blast#6645 / BlastPipe made the scrolling work lol
							Button.BackgroundColor3 = COL3RGB(33, 35, 47)  -- blast#6645 / BlastPipe made the scrolling work lol
							Button.BorderColor3 = COL3RGB(27, 27, 35)  -- blast#6645 / BlastPipe made the scrolling work lol
							Button.Position = UDIM2(0, 30, 0, 16)  -- blast#6645 / BlastPipe made the scrolling work lol
							Button.Size = UDIM2(0, 175, 0, 17)  -- blast#6645 / BlastPipe made the scrolling work lol
							Button.AutoButtonColor = false  -- blast#6645 / BlastPipe made the scrolling work lol
							Button.Font = Enum.Font.SourceSans  -- blast#6645 / BlastPipe made the scrolling work lol
							Button.Text = ""  -- blast#6645 / BlastPipe made the scrolling work lol
							Button.TextColor3 = COL3RGB(0, 0, 0)  -- blast#6645 / BlastPipe made the scrolling work lol
							Button.TextSize = 14.000  -- blast#6645 / BlastPipe made the scrolling work lol
							Button.BorderSizePixel = 0  -- blast#6645 / BlastPipe made the scrolling work lol
							Button.ZIndex = 6  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
							TextLabel.Parent = Button  -- blast#6645 / BlastPipe made the scrolling work lol
							TextLabel.BackgroundColor3 = COL3RGB(255, 255, 255)  -- blast#6645 / BlastPipe made the scrolling work lol
							TextLabel.BackgroundTransparency = 1.000  -- blast#6645 / BlastPipe made the scrolling work lol
							TextLabel.BorderColor3 = COL3RGB(27, 42, 53)  -- blast#6645 / BlastPipe made the scrolling work lol
							TextLabel.Position = UDIM2(0, 5, 0, -1)  -- blast#6645 / BlastPipe made the scrolling work lol
							TextLabel.Size = UDIM2(-0.21714285, 208, 1, 0)  -- blast#6645 / BlastPipe made the scrolling work lol
							TextLabel.Font = Enum.Font.SourceSansSemibold  -- blast#6645 / BlastPipe made the scrolling work lol
							TextLabel.Text = v  -- blast#6645 / BlastPipe made the scrolling work lol
							TextLabel.TextColor3 = COL3RGB(200, 200, 200)  -- blast#6645 / BlastPipe made the scrolling work lol
							TextLabel.TextSize = 14.000  -- blast#6645 / BlastPipe made the scrolling work lol
							TextLabel.TextXAlignment = Enum.TextXAlignment.Left  -- blast#6645 / BlastPipe made the scrolling work lol
							TextLabel.ZIndex = 6  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
							Button.MouseButton1Down:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
								if TBLFIND(Element.value.Jumbobox, v) then  -- blast#6645 / BlastPipe made the scrolling work lol
									for i,a in pairs(Element.value.Jumbobox) do  -- blast#6645 / BlastPipe made the scrolling work lol
										if a == v then  -- blast#6645 / BlastPipe made the scrolling work lol
											TBLREMOVE(Element.value.Jumbobox, i)  -- blast#6645 / BlastPipe made the scrolling work lol
										end  -- blast#6645 / BlastPipe made the scrolling work lol
									end  -- blast#6645 / BlastPipe made the scrolling work lol
									library:Tween(TextLabel, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextColor3 = COL3RGB(255, 255, 255)})  -- blast#6645 / BlastPipe made the scrolling work lol
								else  -- blast#6645 / BlastPipe made the scrolling work lol
									INSERT(Element.value.Jumbobox, v)  -- blast#6645 / BlastPipe made the scrolling work lol
									library:Tween(TextLabel, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextColor3 = COL3RGB(175, 42, 86)})  -- blast#6645 / BlastPipe made the scrolling work lol
								end  -- blast#6645 / BlastPipe made the scrolling work lol
								updatetext()  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
								values[tabname][sectorname][text] = Element.value  -- blast#6645 / BlastPipe made the scrolling work lol
								callback(Element.value)  -- blast#6645 / BlastPipe made the scrolling work lol
							end)  -- blast#6645 / BlastPipe made the scrolling work lol
							Button.MouseEnter:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
								if not TBLFIND(Element.value.Jumbobox, v) then  -- blast#6645 / BlastPipe made the scrolling work lol
									library:Tween(TextLabel, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextColor3 = COL3RGB(255, 255, 255)})  -- blast#6645 / BlastPipe made the scrolling work lol
								end  -- blast#6645 / BlastPipe made the scrolling work lol
							end)  -- blast#6645 / BlastPipe made the scrolling work lol
