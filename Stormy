								InFrame2 = true  -- blast#6645 / BlastPipe made the scrolling work lol
							end)  -- blast#6645 / BlastPipe made the scrolling work lol
							Keybind.MouseLeave:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
								InFrame2 = false  -- blast#6645 / BlastPipe made the scrolling work lol
							end)  -- blast#6645 / BlastPipe made the scrolling work lol
							game:GetService("UserInputService").InputBegan:Connect(function(input)  -- blast#6645 / BlastPipe made the scrolling work lol
								if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.MouseButton2 and not binding then  -- blast#6645 / BlastPipe made the scrolling work lol
									if Frame.Visible == true and not InFrame and not InFrame2 then  -- blast#6645 / BlastPipe made the scrolling work lol
										Frame.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
									end  -- blast#6645 / BlastPipe made the scrolling work lol
								end  -- blast#6645 / BlastPipe made the scrolling work lol
								if binding then  -- blast#6645 / BlastPipe made the scrolling work lol
									binding = false  -- blast#6645 / BlastPipe made the scrolling work lol
									Keybind.Text = input.KeyCode.Name ~= "Unknown" and input.KeyCode.Name:upper() or input.UserInputType.Name:upper()  -- blast#6645 / BlastPipe made the scrolling work lol
									Keybind.Size = UDIM2(0,txt:GetTextSize(Keybind.Text, 14, Enum.Font.SourceSansSemibold, Vec2(700, 12)).X + 5,0, 12)  -- blast#6645 / BlastPipe made the scrolling work lol
									Element.value.Key = input.KeyCode.Name ~= "Unknown" and input.KeyCode.Name or input.UserInputType.Name  -- blast#6645 / BlastPipe made the scrolling work lol
									if input.KeyCode.Name == "Backspace" then  -- blast#6645 / BlastPipe made the scrolling work lol
										Keybind.Text = "NONE"  -- blast#6645 / BlastPipe made the scrolling work lol
										Keybind.Size = UDIM2(0,txt:GetTextSize(Keybind.Text, 14, Enum.Font.SourceSansSemibold, Vec2(700, 12)).X + 4,0, 12)  -- blast#6645 / BlastPipe made the scrolling work lol
										Element.value.Key = nil  -- blast#6645 / BlastPipe made the scrolling work lol
									end  -- blast#6645 / BlastPipe made the scrolling work lol
								else  -- blast#6645 / BlastPipe made the scrolling work lol
									if Element.value.Key ~= nil then  -- blast#6645 / BlastPipe made the scrolling work lol
										if FIND(Element.value.Key, "Mouse") then  -- blast#6645 / BlastPipe made the scrolling work lol
											if input.UserInputType == Enum.UserInputType[Element.value.Key] then  -- blast#6645 / BlastPipe made the scrolling work lol
												if Element.value.Type == "Hold" then  -- blast#6645 / BlastPipe made the scrolling work lol
													Element.value.Active = true  -- blast#6645 / BlastPipe made the scrolling work lol
