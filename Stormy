						local function update()  -- blast#6645 / BlastPipe made the scrolling work lol
							if Element.value.Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
								tween = library:Tween(Color, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = COL3RGB(155, 155, 155)})  -- blast#6645 / BlastPipe made the scrolling work lol
								library:Tween(TextLabel, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextColor3 = COL3RGB(255, 255, 255)})  -- blast#6645 / BlastPipe made the scrolling work lol
							else  -- blast#6645 / BlastPipe made the scrolling work lol
								tween = library:Tween(Color, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = COL3RGB(33, 35, 47)})  -- blast#6645 / BlastPipe made the scrolling work lol
								library:Tween(TextLabel, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextColor3 = COL3RGB(200, 200, 200)})  -- blast#6645 / BlastPipe made the scrolling work lol
							end  -- blast#6645 / BlastPipe made the scrolling work lol
							values[tabname][sectorname][tabtext][text] = Element.value  -- blast#6645 / BlastPipe made the scrolling work lol
							callback(Element.value)  -- blast#6645 / BlastPipe made the scrolling work lol
						end  -- blast#6645 / BlastPipe made the scrolling work lol
