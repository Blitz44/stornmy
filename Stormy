-- blast#6645 / BlastPipe made the scrolling work lol
local function VectorRGB(RGB)  -- blast#6645 / BlastPipe made the scrolling work lol
	return Vec3(RGB.R, RGB.G, RGB.B)  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
local function new(name, prop)  -- blast#6645 / BlastPipe made the scrolling work lol
	local obj = INST(name)  -- blast#6645 / BlastPipe made the scrolling work lol
	for i,v in pairs(prop) do  -- blast#6645 / BlastPipe made the scrolling work lol
		if i ~= "Parent" then  -- blast#6645 / BlastPipe made the scrolling work lol
			obj[i] = v  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	if prop["Parent"] ~= nil then  -- blast#6645 / BlastPipe made the scrolling work lol
		obj.Parent = prop["Parent"]  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
local function UpdateAccessory(Accessory)  -- blast#6645 / BlastPipe made the scrolling work lol
	Accessory.Material = values.visuals.effects["accessory material"].Dropdown == "Smooth" and "SmoothPlastic" or "ForceField"  -- blast#6645 / BlastPipe made the scrolling work lol
	Accessory.Mesh.VertexColor = VectorRGB(values.visuals.effects["accessory chams"].Color)  -- blast#6645 / BlastPipe made the scrolling work lol
	Accessory.Color = values.visuals.effects["accessory chams"].Color  -- blast#6645 / BlastPipe made the scrolling work lol
	Accessory.Transparency = values.visuals.effects["accessory chams"].Transparency  -- blast#6645 / BlastPipe made the scrolling work lol
	if values.visuals.effects["accessory material"].Dropdown ~= "ForceField" then  -- blast#6645 / BlastPipe made the scrolling work lol
		Accessory.Mesh.TextureId = ""  -- blast#6645 / BlastPipe made the scrolling work lol
	else  -- blast#6645 / BlastPipe made the scrolling work lol
		Accessory.Mesh.TextureId = Accessory.StringValue.Value  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
local function ReverseAccessory(Accessory)  -- blast#6645 / BlastPipe made the scrolling work lol
	Accessory.Material = "SmoothPlastic"  -- blast#6645 / BlastPipe made the scrolling work lol
	Accessory.Mesh.VertexColor = Vec3(1,1,1)  -- blast#6645 / BlastPipe made the scrolling work lol
	Accessory.Mesh.TextureId = Accessory.StringValue.Value  -- blast#6645 / BlastPipe made the scrolling work lol
	Accessory.Transparency = 0  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
local function UpdateWeapon(obj)  -- blast#6645 / BlastPipe made the scrolling work lol
	local selected = values.visuals.effects["weapon material"].Dropdown  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
	if obj:IsA("MeshPart") then obj.TextureID = "" end  -- blast#6645 / BlastPipe made the scrolling work lol
	if obj:IsA("Part") and obj:FindFirstChild("Mesh") and not obj:IsA("BlockMesh") then  -- blast#6645 / BlastPipe made the scrolling work lol
		obj.Mesh.VertexColor = VectorRGB(values.visuals.effects["weapon chams"].Color)  -- blast#6645 / BlastPipe made the scrolling work lol
		if selected == "Smooth" or selected == "Glass" then  -- blast#6645 / BlastPipe made the scrolling work lol
			obj.Mesh.TextureId = ""  -- blast#6645 / BlastPipe made the scrolling work lol
		else  -- blast#6645 / BlastPipe made the scrolling work lol
			pcall(function()  -- blast#6645 / BlastPipe made the scrolling work lol
				obj.Mesh.TextureId = obj.Mesh.OriginalTexture.Value  -- blast#6645 / BlastPipe made the scrolling work lol
				obj.Mesh.TextureID = obj.Mesh.OriginalTexture.Value  -- blast#6645 / BlastPipe made the scrolling work lol
			end)  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	obj.Color = values.visuals.effects["weapon chams"].Color  -- blast#6645 / BlastPipe made the scrolling work lol
	obj.Material = selected == "Smooth" and "SmoothPlastic" or selected == "Flat" and "Neon" or selected == "ForceField" and "ForceField" or "Glass"  -- blast#6645 / BlastPipe made the scrolling work lol
	obj.Reflectance = values.visuals.effects["reflectance"].Slider/10  -- blast#6645 / BlastPipe made the scrolling work lol
	obj.Transparency = values.visuals.effects["weapon chams"].Transparency  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
local Skins = ReplicatedStorage.Skins  -- blast#6645 / BlastPipe made the scrolling work lol
local function MapSkin(Gun, Skin, CustomSkin)  -- blast#6645 / BlastPipe made the scrolling work lol
	if CustomSkin ~= nil then  -- blast#6645 / BlastPipe made the scrolling work lol
		for _,Data in pairs(CustomSkin) do  -- blast#6645 / BlastPipe made the scrolling work lol
			local Obj = Camera.Arms:FindFirstChild(Data.Name)  -- blast#6645 / BlastPipe made the scrolling work lol
			if Obj ~= nil and Obj.Transparency ~= 1 then  -- blast#6645 / BlastPipe made the scrolling work lol
				Obj.TextureId = Data.Value  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	else  -- blast#6645 / BlastPipe made the scrolling work lol
		local SkinData = Skins:FindFirstChild(Gun):FindFirstChild(Skin)  -- blast#6645 / BlastPipe made the scrolling work lol
		if not SkinData:FindFirstChild("Animated") then  -- blast#6645 / BlastPipe made the scrolling work lol
			for _,Data in pairs(SkinData:GetChildren()) do  -- blast#6645 / BlastPipe made the scrolling work lol
				local Obj = Camera.Arms:FindFirstChild(Data.Name)  -- blast#6645 / BlastPipe made the scrolling work lol
				if Obj ~= nil and Obj.Transparency ~= 1 then  -- blast#6645 / BlastPipe made the scrolling work lol
					if Obj:FindFirstChild("Mesh") then  -- blast#6645 / BlastPipe made the scrolling work lol
						Obj.Mesh.TextureId = v.Value  -- blast#6645 / BlastPipe made the scrolling work lol
					elseif not Obj:FindFirstChild("Mesh") then  -- blast#6645 / BlastPipe made the scrolling work lol
						Obj.TextureID = Data.Value  -- blast#6645 / BlastPipe made the scrolling work lol
					end  -- blast#6645 / BlastPipe made the scrolling work lol
				end  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
local function ChangeCharacter(NewCharacter)  -- blast#6645 / BlastPipe made the scrolling work lol
	for _,Part in pairs (LocalPlayer.Character:GetChildren()) do  -- blast#6645 / BlastPipe made the scrolling work lol
		if Part:IsA("Accessory") then  -- blast#6645 / BlastPipe made the scrolling work lol
			Part:Destroy()  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
		if Part:IsA("BasePart") then  -- blast#6645 / BlastPipe made the scrolling work lol
			if NewCharacter:FindFirstChild(Part.Name) then  -- blast#6645 / BlastPipe made the scrolling work lol
				Part.Color = NewCharacter:FindFirstChild(Part.Name).Color  -- blast#6645 / BlastPipe made the scrolling work lol
				Part.Transparency = NewCharacter:FindFirstChild(Part.Name).Transparency  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
			if Part.Name == "FakeHead" then  -- blast#6645 / BlastPipe made the scrolling work lol
				Part.Color = NewCharacter:FindFirstChild("Head").Color  -- blast#6645 / BlastPipe made the scrolling work lol
				Part.Transparency = NewCharacter:FindFirstChild("Head").Transparency  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
		if (Part.Name == "Head" or Part.Name == "FakeHead") and Part:FindFirstChildOfClass("Decal") and NewCharacter.Head:FindFirstChildOfClass("Decal") then  -- blast#6645 / BlastPipe made the scrolling work lol
			Part:FindFirstChildOfClass("Decal").Texture = NewCharacter.Head:FindFirstChildOfClass("Decal").Texture  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
	if NewCharacter:FindFirstChildOfClass("Shirt") then  -- blast#6645 / BlastPipe made the scrolling work lol
		if LocalPlayer.Character:FindFirstChildOfClass("Shirt") then  -- blast#6645 / BlastPipe made the scrolling work lol
			LocalPlayer.Character:FindFirstChildOfClass("Shirt"):Destroy()  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
		local Clone = NewCharacter:FindFirstChildOfClass("Shirt"):Clone()  -- blast#6645 / BlastPipe made the scrolling work lol
		Clone.Parent = LocalPlayer.Character  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
	if NewCharacter:FindFirstChildOfClass("Pants") then  -- blast#6645 / BlastPipe made the scrolling work lol
		if LocalPlayer.Character:FindFirstChildOfClass("Pants") then  -- blast#6645 / BlastPipe made the scrolling work lol
			LocalPlayer.Character:FindFirstChildOfClass("Pants"):Destroy()  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
		local Clone = NewCharacter:FindFirstChildOfClass("Pants"):Clone()  -- blast#6645 / BlastPipe made the scrolling work lol
		Clone.Parent = LocalPlayer.Character  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
	for _,Part in pairs (NewCharacter:GetChildren()) do  -- blast#6645 / BlastPipe made the scrolling work lol
		if Part:IsA("Accessory") then  -- blast#6645 / BlastPipe made the scrolling work lol
			local Clone = Part:Clone()  -- blast#6645 / BlastPipe made the scrolling work lol
			for _,Weld in pairs (Clone.Handle:GetChildren()) do  -- blast#6645 / BlastPipe made the scrolling work lol
				if Weld:IsA("Weld") and Weld.Part1 ~= nil then  -- blast#6645 / BlastPipe made the scrolling work lol
					Weld.Part1 = LocalPlayer.Character[Weld.Part1.Name]  -- blast#6645 / BlastPipe made the scrolling work lol
				end  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
			Clone.Parent = LocalPlayer.Character  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
	if LocalPlayer.Character:FindFirstChildOfClass("Shirt") then  -- blast#6645 / BlastPipe made the scrolling work lol
		local String = INST("StringValue")  -- blast#6645 / BlastPipe made the scrolling work lol
		String.Name = "OriginalTexture"  -- blast#6645 / BlastPipe made the scrolling work lol
		String.Value = LocalPlayer.Character:FindFirstChildOfClass("Shirt").ShirtTemplate  -- blast#6645 / BlastPipe made the scrolling work lol
		String.Parent = LocalPlayer.Character:FindFirstChildOfClass("Shirt")  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
		if TBLFIND(values.visuals.effects.removals.Jumbobox, "clothes") then  -- blast#6645 / BlastPipe made the scrolling work lol
			LocalPlayer.Character:FindFirstChildOfClass("Shirt").ShirtTemplate = ""  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	if LocalPlayer.Character:FindFirstChildOfClass("Pants") then  -- blast#6645 / BlastPipe made the scrolling work lol
		local String = INST("StringValue")  -- blast#6645 / BlastPipe made the scrolling work lol
		String.Name = "OriginalTexture"  -- blast#6645 / BlastPipe made the scrolling work lol
		String.Value = LocalPlayer.Character:FindFirstChildOfClass("Pants").PantsTemplate  -- blast#6645 / BlastPipe made the scrolling work lol
		String.Parent = LocalPlayer.Character:FindFirstChildOfClass("Pants")  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
		if TBLFIND(values.visuals.effects.removals.Jumbobox, "clothes") then  -- blast#6645 / BlastPipe made the scrolling work lol
			LocalPlayer.Character:FindFirstChildOfClass("Pants").PantsTemplate = ""  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
	for i,v in pairs(LocalPlayer.Character:GetChildren()) do  -- blast#6645 / BlastPipe made the scrolling work lol
		if v:IsA("BasePart") and v.Transparency ~= 1 then  -- blast#6645 / BlastPipe made the scrolling work lol
			INSERT(SelfObj, v)  -- blast#6645 / BlastPipe made the scrolling work lol
			local Color = INST("Color3Value")  -- blast#6645 / BlastPipe made the scrolling work lol
			Color.Name = "OriginalColor"  -- blast#6645 / BlastPipe made the scrolling work lol
			Color.Value = v.Color  -- blast#6645 / BlastPipe made the scrolling work lol
			Color.Parent = v  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
			local String = INST("StringValue")  -- blast#6645 / BlastPipe made the scrolling work lol
			String.Name = "OriginalMaterial"  -- blast#6645 / BlastPipe made the scrolling work lol
			String.Value = v.Material.Name  -- blast#6645 / BlastPipe made the scrolling work lol
			String.Parent = v  -- blast#6645 / BlastPipe made the scrolling work lol
		elseif v:IsA("Accessory") and v.Handle.Transparency ~= 1 then  -- blast#6645 / BlastPipe made the scrolling work lol
			INSERT(SelfObj, v.Handle)  -- blast#6645 / BlastPipe made the scrolling work lol
			local Color = INST("Color3Value")  -- blast#6645 / BlastPipe made the scrolling work lol
			Color.Name = "OriginalColor"  -- blast#6645 / BlastPipe made the scrolling work lol
			Color.Value = v.Handle.Color  -- blast#6645 / BlastPipe made the scrolling work lol
			Color.Parent = v.Handle  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
			local String = INST("StringValue")  -- blast#6645 / BlastPipe made the scrolling work lol
			String.Name = "OriginalMaterial"  -- blast#6645 / BlastPipe made the scrolling work lol
			String.Value = v.Handle.Material.Name  -- blast#6645 / BlastPipe made the scrolling work lol
			String.Parent = v.Handle  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
	if values.visuals.self["self chams"].Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
		for _,obj in pairs(SelfObj) do  -- blast#6645 / BlastPipe made the scrolling work lol
			if obj.Parent ~= nil then  -- blast#6645 / BlastPipe made the scrolling work lol
				obj.Material = Enum.Material.ForceField  -- blast#6645 / BlastPipe made the scrolling work lol
				obj.Color = values.visuals.self["self chams"].Color  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
local function GetDeg(pos1, pos2)  -- blast#6645 / BlastPipe made the scrolling work lol
	local start = pos1.LookVector  -- blast#6645 / BlastPipe made the scrolling work lol
	local vector = CF(pos1.Position, pos2).LookVector  -- blast#6645 / BlastPipe made the scrolling work lol
	local angle = ACOS(start:Dot(vector))  -- blast#6645 / BlastPipe made the scrolling work lol
	local deg = DEG(angle)  -- blast#6645 / BlastPipe made the scrolling work lol
	return deg  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
