					values[tabname][sectorname][text] = Element.value  -- blast#6645 / BlastPipe made the scrolling work lol
					function Element:SetValue(value)  -- blast#6645 / BlastPipe made the scrolling work lol
						Element.value = value  -- blast#6645 / BlastPipe made the scrolling work lol
						local duplicate = COL3(value.Color.R, value.Color.G, value.Color.B)  -- blast#6645 / BlastPipe made the scrolling work lol
						ColorH, ColorS, ColorV = duplicate:ToHSV()  -- blast#6645 / BlastPipe made the scrolling work lol
						ColorH = CLAMP(ColorH,0,1)  -- blast#6645 / BlastPipe made the scrolling work lol
						ColorS = CLAMP(ColorS,0,1)  -- blast#6645 / BlastPipe made the scrolling work lol
						ColorV = CLAMP(ColorV,0,1)  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
						ColorDrag.Position = UDIM2(1-ColorS,0,1-ColorV,0)  -- blast#6645 / BlastPipe made the scrolling work lol
						Colorpick.ImageColor3 = COL3HSV(ColorH, 1, 1)  -- blast#6645 / BlastPipe made the scrolling work lol
						ColorP.BackgroundColor3 = COL3HSV(ColorH, ColorS, ColorV)  -- blast#6645 / BlastPipe made the scrolling work lol
						update()  -- blast#6645 / BlastPipe made the scrolling work lol
						Huedrag.Position = UDIM2(0, 0, 1-ColorH, -1)  -- blast#6645 / BlastPipe made the scrolling work lol
					end  -- blast#6645 / BlastPipe made the scrolling work lol
				elseif type == "TextBox" then  -- blast#6645 / BlastPipe made the scrolling work lol
					Section.Size = Section.Size + UDIM2(0,0,0,30)  -- blast#6645 / BlastPipe made the scrolling work lol
					Element.value = {Text = data.default and data.default.text or ""}  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
