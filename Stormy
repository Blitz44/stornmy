local crosshaireditor = misc:Sector("crosshair editor", "Right")  -- blast#6645 / BlastPipe made the scrolling work lol
local function UpdateCrosshair()  -- blast#6645 / BlastPipe made the scrolling work lol
	if values.misc["crosshair editor"].enabled.Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
		local length = values.misc["crosshair editor"].length.Slider  -- blast#6645 / BlastPipe made the scrolling work lol
		Crosshair.LeftFrame.Size = UDIM2(0, length, 0, 2)  -- blast#6645 / BlastPipe made the scrolling work lol
		Crosshair.RightFrame.Size = UDIM2(0, length, 0, 2)  -- blast#6645 / BlastPipe made the scrolling work lol
		Crosshair.TopFrame.Size = UDIM2(0, 2, 0, length)  -- blast#6645 / BlastPipe made the scrolling work lol
		Crosshair.BottomFrame.Size = UDIM2(0, 2, 0, length)  -- blast#6645 / BlastPipe made the scrolling work lol
		for _,frame in pairs(Crosshair:GetChildren()) do  -- blast#6645 / BlastPipe made the scrolling work lol
			if FIND(frame.Name, "Frame") then  -- blast#6645 / BlastPipe made the scrolling work lol
				frame.BorderColor3 = COL3(0,0,0)  -- blast#6645 / BlastPipe made the scrolling work lol
				if values.misc["crosshair editor"].border.Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
					frame.BorderSizePixel = 1  -- blast#6645 / BlastPipe made the scrolling work lol
				else  -- blast#6645 / BlastPipe made the scrolling work lol
					frame.BorderSizePixel = 0  -- blast#6645 / BlastPipe made the scrolling work lol
				end  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	else  -- blast#6645 / BlastPipe made the scrolling work lol
		Crosshair.LeftFrame.Size = UDIM2(0, 10, 0, 2)  -- blast#6645 / BlastPipe made the scrolling work lol
		Crosshair.RightFrame.Size = UDIM2(0, 10, 0, 2)  -- blast#6645 / BlastPipe made the scrolling work lol
		Crosshair.TopFrame.Size = UDIM2(0, 2, 0, 10)  -- blast#6645 / BlastPipe made the scrolling work lol
		Crosshair.BottomFrame.Size = UDIM2(0, 2, 0, 10)  -- blast#6645 / BlastPipe made the scrolling work lol
		for _,frame in pairs(Crosshair:GetChildren()) do  -- blast#6645 / BlastPipe made the scrolling work lol
			if FIND(frame.Name, "Frame") then  -- blast#6645 / BlastPipe made the scrolling work lol
				frame.BorderSizePixel = 0  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
end  -- blast#6645 / BlastPipe made the scrolling work lol
crosshaireditor:Element("Toggle", "enabled", nil, UpdateCrosshair)  -- blast#6645 / BlastPipe made the scrolling work lol
crosshaireditor:Element("Slider", "length", {min = 1, max = 15, default = 10}, UpdateCrosshair)  -- blast#6645 / BlastPipe made the scrolling work lol
crosshaireditor:Element("Toggle", "border", nil, UpdateCrosshair)  -- blast#6645 / BlastPipe made the scrolling work lol
