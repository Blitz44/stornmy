					VisibleCham.AlwaysOnTop = false  -- blast#6645 / BlastPipe made the scrolling work lol
					VisibleCham.ZIndex = 8  -- blast#6645 / BlastPipe made the scrolling work lol
					VisibleCham.Size = obj.Size + Vec3(0.1,0.1,0.1)  -- blast#6645 / BlastPipe made the scrolling work lol
					VisibleCham.AlwaysOnTop = false  -- blast#6645 / BlastPipe made the scrolling work lol
					VisibleCham.Transparency = 0  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
					local WallCham = INST("BoxHandleAdornment")  -- blast#6645 / BlastPipe made the scrolling work lol
					WallCham.Name = "WallCham"  -- blast#6645 / BlastPipe made the scrolling work lol
					WallCham.AlwaysOnTop = true  -- blast#6645 / BlastPipe made the scrolling work lol
					WallCham.ZIndex = 5  -- blast#6645 / BlastPipe made the scrolling work lol
					WallCham.Size = obj.Size + Vec3(0.1,0.1,0.1)  -- blast#6645 / BlastPipe made the scrolling work lol
					WallCham.AlwaysOnTop = true  -- blast#6645 / BlastPipe made the scrolling work lol
					WallCham.Transparency = 0.7  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
					if values.visuals.players.chams.Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
						if values.visuals.players.teammates.Toggle or Player.Team ~= LocalPlayer.Team then  -- blast#6645 / BlastPipe made the scrolling work lol
							VisibleCham.Visible = true  -- blast#6645 / BlastPipe made the scrolling work lol
							WallCham.Visible = true  -- blast#6645 / BlastPipe made the scrolling work lol
						else  -- blast#6645 / BlastPipe made the scrolling work lol
							VisibleCham.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
							WallCham.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
						end  -- blast#6645 / BlastPipe made the scrolling work lol
					else  -- blast#6645 / BlastPipe made the scrolling work lol
						VisibleCham.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
						WallCham.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
					end  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
					INSERT(ChamItems, VisibleCham)  -- blast#6645 / BlastPipe made the scrolling work lol
					INSERT(ChamItems, WallCham)  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
					VisibleCham.Color3 = values.visuals.players.chams.Color  -- blast#6645 / BlastPipe made the scrolling work lol
					WallCham.Color3 = values.visuals.players.chams.Color  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
					VisibleCham.AdornCullingMode = "Never"  -- blast#6645 / BlastPipe made the scrolling work lol
					WallCham.AdornCullingMode = "Never"  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
					VisibleCham.Adornee = obj  -- blast#6645 / BlastPipe made the scrolling work lol
					VisibleCham.Parent = obj  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
					WallCham.Adornee = obj  -- blast#6645 / BlastPipe made the scrolling work lol
					WallCham.Parent = obj  -- blast#6645 / BlastPipe made the scrolling work lol
