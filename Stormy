local effects = visuals:Sector("effects", "Right")  -- blast#6645 / BlastPipe made the scrolling work lol
effects:Element("ToggleTrans", "weapon chams", {default = {Color = COL3RGB(255,255,255), Transparency = 0}}, function(tbl)  -- blast#6645 / BlastPipe made the scrolling work lol
	if WeaponObj == nil then return end  -- blast#6645 / BlastPipe made the scrolling work lol
	if tbl.Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
		for i,v in pairs(WeaponObj) do  -- blast#6645 / BlastPipe made the scrolling work lol
			UpdateWeapon(v)  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	else  -- blast#6645 / BlastPipe made the scrolling work lol
		for i,v in pairs(WeaponObj) do  -- blast#6645 / BlastPipe made the scrolling work lol
			if v:IsA("MeshPart") then v.TextureID = v.OriginalTexture.Value end  -- blast#6645 / BlastPipe made the scrolling work lol
			if v:IsA("Part") and v:FindFirstChild("Mesh") and not v:IsA("BlockMesh") then  -- blast#6645 / BlastPipe made the scrolling work lol
				v.Mesh.TextureId = v.Mesh.OriginalTexture.Value  -- blast#6645 / BlastPipe made the scrolling work lol
				v.Mesh.VertexColor = Vec3(1,1,1)  -- blast#6645 / BlastPipe made the scrolling work lol
			end  -- blast#6645 / BlastPipe made the scrolling work lol
			v.Color = v.OriginalColor.Value  -- blast#6645 / BlastPipe made the scrolling work lol
			v.Material = v.OriginalMaterial.Value  -- blast#6645 / BlastPipe made the scrolling work lol
			v.Transparency = 0  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
	end  -- blast#6645 / BlastPipe made the scrolling work lol
end)  -- blast#6645 / BlastPipe made the scrolling work lol
