effects:Element("ToggleTrans", "accessory chams", {default = {Color = COL3RGB(255,255,255)}}, function(val)  -- blast#6645 / BlastPipe made the scrolling work lol
	if RArm == nil or LArm == nil then return end  -- blast#6645 / BlastPipe made the scrolling work lol
	if val.Toggle then  -- blast#6645 / BlastPipe made the scrolling work lol
		if RGlove ~= nil then  -- blast#6645 / BlastPipe made the scrolling work lol
			UpdateAccessory(RGlove)  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
		if RSleeve ~= nil then  -- blast#6645 / BlastPipe made the scrolling work lol
			UpdateAccessory(RSleeve)  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
		if LGlove ~= nil then  -- blast#6645 / BlastPipe made the scrolling work lol
			UpdateAccessory(LGlove)  -- blast#6645 / BlastPipe made the scrolling work lol
		end  -- blast#6645 / BlastPipe made the scrolling work lol
