-- blast#6645 / BlastPipe made the scrolling work lol
						local function updatetext()  -- blast#6645 / BlastPipe made the scrolling work lol
							local old = {}  -- blast#6645 / BlastPipe made the scrolling work lol
							for i,v in ipairs(data.options) do  -- blast#6645 / BlastPipe made the scrolling work lol
								if TBLFIND(Element.value.Jumbobox, v) then  -- blast#6645 / BlastPipe made the scrolling work lol
									INSERT(old, v)  -- blast#6645 / BlastPipe made the scrolling work lol
								else  -- blast#6645 / BlastPipe made the scrolling work lol
								end  -- blast#6645 / BlastPipe made the scrolling work lol
							end  -- blast#6645 / BlastPipe made the scrolling work lol
							local str = ""  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
							if #old == 0 then  -- blast#6645 / BlastPipe made the scrolling work lol
								str = "..."  -- blast#6645 / BlastPipe made the scrolling work lol
							else  -- blast#6645 / BlastPipe made the scrolling work lol
