-- blast#6645 / BlastPipe made the scrolling work lol
						local mouse = LocalPlayer:GetMouse()  -- blast#6645 / BlastPipe made the scrolling work lol
						game:GetService("UserInputService").InputBegan:Connect(function(input)  -- blast#6645 / BlastPipe made the scrolling work lol
							if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.MouseButton2 then  -- blast#6645 / BlastPipe made the scrolling work lol
								if not dragging and not abc and not inCP then  -- blast#6645 / BlastPipe made the scrolling work lol
									Frame.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
								end  -- blast#6645 / BlastPipe made the scrolling work lol
							end  -- blast#6645 / BlastPipe made the scrolling work lol
						end)  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
						local function updateColor()  -- blast#6645 / BlastPipe made the scrolling work lol
							local ColorX = (CLAMP(mouse.X - Colorpick.AbsolutePosition.X, 0, Colorpick.AbsoluteSize.X)/Colorpick.AbsoluteSize.X)  -- blast#6645 / BlastPipe made the scrolling work lol
							local ColorY = (CLAMP(mouse.Y - Colorpick.AbsolutePosition.Y, 0, Colorpick.AbsoluteSize.Y)/Colorpick.AbsoluteSize.Y)  -- blast#6645 / BlastPipe made the scrolling work lol
							ColorDrag.Position = UDIM2(ColorX, 0, ColorY, 0)  -- blast#6645 / BlastPipe made the scrolling work lol
							ColorS = 1-ColorX  -- blast#6645 / BlastPipe made the scrolling work lol
							ColorV = 1-ColorY  -- blast#6645 / BlastPipe made the scrolling work lol
							Colorpick.ImageColor3 = COL3HSV(ColorH, 1, 1)  -- blast#6645 / BlastPipe made the scrolling work lol
							ColorP.BackgroundColor3 = COL3HSV(ColorH, ColorS, ColorV)  -- blast#6645 / BlastPipe made the scrolling work lol
							values[tabname][sectorname][tabtext][text] = Element.value  -- blast#6645 / BlastPipe made the scrolling work lol
							Element.value.Color = COL3HSV(ColorH, ColorS, ColorV)  -- blast#6645 / BlastPipe made the scrolling work lol
							callback(Element.value)  -- blast#6645 / BlastPipe made the scrolling work lol
						end  -- blast#6645 / BlastPipe made the scrolling work lol
						local function updateHue()  -- blast#6645 / BlastPipe made the scrolling work lol
							local y = CLAMP(mouse.Y - Huepick.AbsolutePosition.Y, 0, 148)  -- blast#6645 / BlastPipe made the scrolling work lol
							Huedrag.Position = UDIM2(0, 0, 0, y)  -- blast#6645 / BlastPipe made the scrolling work lol
							hue = y/148  -- blast#6645 / BlastPipe made the scrolling work lol
							ColorH = 1-hue  -- blast#6645 / BlastPipe made the scrolling work lol
							Colorpick.ImageColor3 = COL3HSV(ColorH, 1, 1)  -- blast#6645 / BlastPipe made the scrolling work lol
							ColorP.BackgroundColor3 = COL3HSV(ColorH, ColorS, ColorV)  -- blast#6645 / BlastPipe made the scrolling work lol
							values[tabname][sectorname][tabtext][text] = Element.value  -- blast#6645 / BlastPipe made the scrolling work lol
							Element.value.Color = COL3HSV(ColorH, ColorS, ColorV)  -- blast#6645 / BlastPipe made the scrolling work lol
							callback(Element.value)  -- blast#6645 / BlastPipe made the scrolling work lol
						end  -- blast#6645 / BlastPipe made the scrolling work lol
						Colorpick.MouseButton1Down:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
							updateColor()  -- blast#6645 / BlastPipe made the scrolling work lol
							moveconnection = mouse.Move:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
								updateColor()  -- blast#6645 / BlastPipe made the scrolling work lol
							end)  -- blast#6645 / BlastPipe made the scrolling work lol
							releaseconnection = game:GetService("UserInputService").InputEnded:Connect(function(Mouse)  -- blast#6645 / BlastPipe made the scrolling work lol
								if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then  -- blast#6645 / BlastPipe made the scrolling work lol
									updateColor()  -- blast#6645 / BlastPipe made the scrolling work lol
									moveconnection:Disconnect()  -- blast#6645 / BlastPipe made the scrolling work lol
									releaseconnection:Disconnect()  -- blast#6645 / BlastPipe made the scrolling work lol
								end  -- blast#6645 / BlastPipe made the scrolling work lol
							end)  -- blast#6645 / BlastPipe made the scrolling work lol
						end)  -- blast#6645 / BlastPipe made the scrolling work lol
						Huepick.MouseButton1Down:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
							updateHue()  -- blast#6645 / BlastPipe made the scrolling work lol
							moveconnection = mouse.Move:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
								updateHue()  -- blast#6645 / BlastPipe made the scrolling work lol
							end)  -- blast#6645 / BlastPipe made the scrolling work lol
							releaseconnection = game:GetService("UserInputService").InputEnded:Connect(function(Mouse)  -- blast#6645 / BlastPipe made the scrolling work lol
								if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then  -- blast#6645 / BlastPipe made the scrolling work lol
									updateHue()  -- blast#6645 / BlastPipe made the scrolling work lol
									moveconnection:Disconnect()  -- blast#6645 / BlastPipe made the scrolling work lol
									releaseconnection:Disconnect()  -- blast#6645 / BlastPipe made the scrolling work lol
								end  -- blast#6645 / BlastPipe made the scrolling work lol
							end)  -- blast#6645 / BlastPipe made the scrolling work lol
						end)  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
						Button.MouseButton1Down:Connect(function()  -- blast#6645 / BlastPipe made the scrolling work lol
							Element.value.Toggle = not Element.value.Toggle  -- blast#6645 / BlastPipe made the scrolling work lol
							update()  -- blast#6645 / BlastPipe made the scrolling work lol
							values[tabname][sectorname][tabtext][text] = Element.value  -- blast#6645 / BlastPipe made the scrolling work lol
							callback(Element.value)  -- blast#6645 / BlastPipe made the scrolling work lol
						end)  -- blast#6645 / BlastPipe made the scrolling work lol
						if data.default then  -- blast#6645 / BlastPipe made the scrolling work lol
							update()  -- blast#6645 / BlastPipe made the scrolling work lol
						end  -- blast#6645 / BlastPipe made the scrolling work lol
						values[tabname][sectorname][tabtext][text] = Element.value  -- blast#6645 / BlastPipe made the scrolling work lol
						function Element:SetValue(value)  -- blast#6645 / BlastPipe made the scrolling work lol
							Element.value = value  -- blast#6645 / BlastPipe made the scrolling work lol
							local duplicate = COL3(value.Color.R, value.Color.G, value.Color.B)  -- blast#6645 / BlastPipe made the scrolling work lol
							ColorH, ColorS, ColorV = duplicate:ToHSV()  -- blast#6645 / BlastPipe made the scrolling work lol
							ColorH = CLAMP(ColorH,0,1)  -- blast#6645 / BlastPipe made the scrolling work lol
							ColorS = CLAMP(ColorS,0,1)  -- blast#6645 / BlastPipe made the scrolling work lol
							ColorV = CLAMP(ColorV,0,1)  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
							ColorDrag.Position = UDIM2(1-ColorS,0,1-ColorV,0)  -- blast#6645 / BlastPipe made the scrolling work lol
							Colorpick.ImageColor3 = COL3HSV(ColorH, 1, 1)  -- blast#6645 / BlastPipe made the scrolling work lol
							ColorP.BackgroundColor3 = COL3HSV(ColorH, ColorS, ColorV)  -- blast#6645 / BlastPipe made the scrolling work lol
							update()  -- blast#6645 / BlastPipe made the scrolling work lol
							Huedrag.Position = UDIM2(0, 0, 1-ColorH, -1)  -- blast#6645 / BlastPipe made the scrolling work lol
						end  -- blast#6645 / BlastPipe made the scrolling work lol
					elseif type == "ToggleTrans" then  -- blast#6645 / BlastPipe made the scrolling work lol
						tabsize = tabsize + UDIM2(0,0,0,16)  -- blast#6645 / BlastPipe made the scrolling work lol
						Element.value = {Toggle = data.default and data.default.Toggle or false, Color = data.default and data.default.Color or COL3RGB(255,255,255), Transparency = data.default and data.default.Transparency or 0}  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
						local Toggle = INST("Frame")  -- blast#6645 / BlastPipe made the scrolling work lol
						local Button = INST("TextButton")  -- blast#6645 / BlastPipe made the scrolling work lol
						local Color = INST("Frame")  -- blast#6645 / BlastPipe made the scrolling work lol
						local TextLabel = INST("TextLabel")  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
						Toggle.Name = "Toggle"  -- blast#6645 / BlastPipe made the scrolling work lol
						Toggle.Parent = tab1  -- blast#6645 / BlastPipe made the scrolling work lol
						Toggle.BackgroundColor3 = COL3RGB(255, 255, 255)  -- blast#6645 / BlastPipe made the scrolling work lol
						Toggle.BackgroundTransparency = 1.000  -- blast#6645 / BlastPipe made the scrolling work lol
						Toggle.Size = UDIM2(1, 0, 0, 15)  -- blast#6645 / BlastPipe made the scrolling work lol
-- blast#6645 / BlastPipe made the scrolling work lol
