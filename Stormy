						local mouse = LocalPlayer:GetMouse()  -- blast#6645 / BlastPipe made the scrolling work lol
						game:GetService("UserInputService").InputBegan:Connect(function(input)  -- blast#6645 / BlastPipe made the scrolling work lol
							if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.MouseButton2 then  -- blast#6645 / BlastPipe made the scrolling work lol
								if not dragging and not abc and not inCP then  -- blast#6645 / BlastPipe made the scrolling work lol
									Frame.Visible = false  -- blast#6645 / BlastPipe made the scrolling work lol
								end  -- blast#6645 / BlastPipe made the scrolling work lol
